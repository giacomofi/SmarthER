import { IAssetProvider } from './IAssetProvider.sol';

interface ISVGHelper {
  // point
  // 0-15: int x
  // 16-31: int y
  // 32-47: uint r
  // 48: bool c
  function pathFromPoints(uint[] memory points) external pure returns(bytes memory);
