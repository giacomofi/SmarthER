    uint256 public partnerBalance;

    // max mintable NFT amount at each stage
    // _MaxMintAmountAtEachStage[stage] = amount
    mapping(uint256 => uint256) private _MaxMintAmountAtEachStage;

    // Token Base URI
    string private _tokenBaseURI;

    // Whitelist for stage 2
    mapping( address => bool ) private whitelist;

    /**
     * @dev Return the start Date of the first stage
     */ 
    function getFirstStageStartDate() public view returns (uint256) {
        return _firstStageStartDate;
    }

    /**
     * @dev Return the start Date of the second stage
     */ 
    function getSecondStageStartDate() public view returns (uint256) {
        return _secondStageStartDate;
    }

    /**
     * @dev Return the start Date of the third stage
     */ 
    function getThirdStageStartDage() public view returns (uint256) {
        return _thirdStageStartDate;
    }

    /**
     * @dev Initialize the start dates of each stages.
     * requirements:
     *  - only Owner can initialize the start
     *  - firstStageStartDate must be bigger than the current timestamp
     *  - secondStageStartDate must be bigger than the firstStageStartDate
     *  - thirdStageStartDate must be bigger than the SecondStageStartDate
     */ 

    function init_stages (uint256 firstStageStartDate_, uint256 secondStageStartDate_, uint256 thirdStageStartDate_) public onlyOwner {
        require(firstStageStartDate_ >= block.timestamp, "init_stages: firstStageStartDate must be bigger than the current timestamp");
        require(secondStageStartDate_ >= firstStageStartDate_, "init_stages: secondStageStartDate must be bigger than firstStageStartDate");
        require(thirdStageStartDate_ >= secondStageStartDate_, "init_stages: thirdStageStartDate must be bigger than secondStageStartDate");
        _firstStageStartDate = firstStageStartDate_;
        _secondStageStartDate = secondStageStartDate_;
        _thirdStageStartDate = thirdStageStartDate_;
    }

    /**
     * @dev Initialize the max mintable amount at each stage
     * @notice Only Owner can call this function
     * input format: 
     *      [10, 10010, 20010]
     */

    function init_maxAmountAtEachStage (uint256[] memory maxAmounts) public onlyOwner {
        for(uint256 i = 0; i < maxAmounts.length; i++) {
            _MaxMintAmountAtEachStage[i] = maxAmounts[i];
        }
    }

    /**
     * @dev Function to set the withdraw percentages of owner and partner
     * input format : [88,12]
     */

    function setPercentages (uint256[] memory percentages) public onlyOwner {
        require(percentages.length == 2, "setPercentages: input length must be 2");
        require(percentages[0] >= 0 && percentages[0] <= 100 && percentages[1] >= 0 && percentages[1] <= 100, "setPercentages: invalid input");
        require(percentages[0] + percentages[1] == 100, "setPercentages: sum of two input values must be equal to 100");
        _ownerPercentage = percentages[0];
        _partnerPercentage = percentages[1];
    }

    function getOwnerPercentage () public view returns (uint256) {
        return _ownerPercentage;
    }

    function getPartnerPercentage () public view returns (uint256) {
        return _partnerPercentage;
    }

    function setPartnerAddress (address partner) public onlyOwner {
        require(partner != address(0), "setPartnerAddress: you can't set the partner's address as zero address");
        _partnerAddress = partner;
    }

    function getPartnerAddress () public view returns (address) {
        return _partnerAddress;
    }

    /**
     * @dev The function to add adderss to the whitelist
     *       - only Owner can register addresses to the whitelist
     *       - wallet address can't be zero address
     */
    function addToWhitelist ( address wallet ) public onlyOwner {
        require(wallet != address(0), "addToWhitelist: can't register zero address to the whitelist");
        whitelist[wallet] = true;
    }

    /**
     * @dev The function to remove address from the whitelist
     *      - only Owner can remove address from the whitelist
     *      - wallet address can't be zero address
     */
    function removeFromWhitelist ( address wallet ) public onlyOwner {
        require(wallet != address(0), "removeFromWhitelist: can't remove zero address from the whitelist");
        whitelist[wallet] = false;
    }

    /**
     * @dev The function to check if the address is whitelisted or not
     *      - the address can't be zero address
     *      - return true if the wallet address is whitelisted
     *      - return false if the wallet address is not whitelisted
     */
    function isWhitelisted ( address wallet ) public view returns (bool) {
        require ( wallet != address(0), "isWhitelisted: the address can't be zero");
        return whitelist[wallet];
    }

    /**
     * @dev Returns the current stage according to the current timestamp
     * 0: pending
     * 1: stage 1 : mint 10 NFTs without whitelist
     * 2: stage 2 : mint 10,000 NFTs with whitelist
     * 3: stage 3 : free airdrop 10,000 NFTs
     * 4: ended
     */
    function getCurrentStage () public view returns (uint256) {
        if(_secondStageStartDate == 0) return 3; // pending
        if (block.timestamp >= _thirdStageStartDate) {
            return 2; // third stage
        }
        if ((block.timestamp < _thirdStageStartDate) && (block.timestamp >= _secondStageStartDate)) {
            return 1; // second stage
        }

        if ((block.timestamp < _secondStageStartDate) && (block.timestamp >= _firstStageStartDate)) {
            return 0; // first stage
        }
        return 3; // pending
    }

    /** 
     * @dev call internal _mint function to mint "quantity" NFT to address "to"
     * @notice to: receiver address
     *         quantity: NFT mint amount
     *  requirements : 
     *     - this function must be payable 
     *     - receiver address can't be zero address
     *     - mint amount must bigger than 0
     *     - mint stage must not be pending which is 0
     *     - Owner can mint freely
     *     - Normal user must pay, msg.value must be equal to NFT price * quantity
     *     - total minted amount at each stage must no exceed the max value of each stage
     *     - at second stage, "to" address must be whitelisted
     */
    function mint (address to, uint256 quantity) public payable {
        require(to != address(0), "mint: You can't mint to zero address");
        require(quantity > 0, "mint: You can't mint 0 NFT");
        require(_price > 0, "mint: Please set the mint price");
        
        uint256 currentStage = getCurrentStage();
        
        require(currentStage != 3, "mint: you can't mint at this time, please wait until phase 0");
        
        if(msg.sender != owner()) {
            require(msg.value >= _price * quantity, "mint: please send enough funds");
        }

        require(_totalMinted() + quantity <=  _MaxMintAmountAtEachStage[currentStage], "mint: not enough NFTs are available at the first stage");

        // need to implement whitelist require here if current stage is 2
        // 
        // write here
        if(currentStage == 1 && msg.sender != owner()) {
            require(whitelist[to] == true, "mint: this address is not whitelisted");
        }
        //

        _mint(owner(), quantity);

        ownerBalance = ownerBalance + _price.mul(quantity).mul(_ownerPercentage).div(100);
        partnerBalance = partnerBalance + _price.mul(_partnerPercentage).div(100);

        // we don't need to add tokenURI part because once the token is minted, then token uri is baseURI/tokenId
    }

    /**
     * @dev override _baseURI() function from ERC721A
     */
    function _baseURI() internal view override returns (string memory) {
        return _tokenBaseURI;
    }

    /**
     * @dev The function to set the base uri
     *      - only Owner can set and change the base URI
     *      - length of the baseURI can't be zero
     */
    function setBaseURI(string memory baseURI) public onlyOwner {
        require(bytes(baseURI).length > 0, "setBaseURI: length of the base URI can't be zero");
        _tokenBaseURI = baseURI;
    }

    /**
     * @dev The function to set the NFT price
     *      - only Owner can change the NFT price
     */
    function setNFTPrice( uint256 price ) public onlyOwner {
        _price = price;
    }

    /**
     * @dev The function to get the NFT price
     */
    function getNFTPrice() public view returns (uint256) {
        return _price;
    }

    /**
     * @dev The function returns MaxMintAmountAtEachStage
     */
    function maxMintAmountAtEachStage (uint256 stage) public view returns (uint256) {
        return _MaxMintAmountAtEachStage[stage];
    }

    /**
     * @dev only Owner can withdraw the balance of this smart contract
     */
    function ownerWithdraw () public onlyOwner {
        address payable to = payable(msg.sender);
        uint256 temp = ownerBalance;
        ownerBalance = 0;
        to.transfer(temp);
    }

    /**
     * @dev partner address can withdraw 
     */
    function partnerWithdraw () public {
        require(_partnerAddress == msg.sender, "partnerWithdraw: you are not the partner of owner");
        address payable to = payable(msg.sender);
        uint256 temp = partnerBalance;
        partnerBalance = 0;
        to.transfer(temp);
    }

}