        usdt = IERC20Upgradeable(0xdAC17F958D2ee523a2206206994597C13D831ec7); 
        uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        usdt.safeApprove(address(uniswapV2Router), type(uint256).max);
        dapp.safeApprove(address(uniswapV2Router), type(uint256).max);
        feeInfo = fees(0,1000,1500,2000,2500,3000,100000);

        isReferralContract[0xDE38C17c1DB31fC8b8da33D3EdFd21C7ac0Ae0b4] = true;
        isReferralContract[0x774607DaeA5B7FB3522F8E63f62ac5573752f3cc] = true;
        isReferralContract[0xfc7A79b3cB02e27D29cD8f4E77bAd50C5bFfADF9] = true;

        currentTicket = 1;
    }
   
    receive() external payable {}

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    } 

    function setBlockedUsers(address account, bool status) public onlyOwner {
        isBlockedUsers[account] = status;
    }

    function usdtIncreaseAllowance() public onlyOwner {
        usdt.safeApprove(address(uniswapV2Router), type(uint256).max);
    }

    function dappIncreaseAllowance() public onlyOwner {
        dapp.safeApprove(address(uniswapV2Router), type(uint256).max);
    }

    function setUsdtToken(address newToken) external onlyOwner {
        usdt = IERC20Upgradeable(newToken);
    }

    function setDappleToken(address newToken) external onlyOwner {
        dapp = IERC20Upgradeable(newToken);
    }

    function setFees(fees calldata newFees) external onlyOwner {
        require(newFees.rewardBasics <= 4, "F");

        feeInfo = newFees;
    }

    function setRewardToken(address rewardToken,bool status) external onlyOwner {
        rewardTokenInfo[rewardToken] = status;
    }

    function usdtBalance() internal view returns (uint256) {
        return usdt.balanceOf(address(this));
    }

    function setReferralContract(address newContract,bool status) external onlyOwner {
        isReferralContract[newContract] = status;
    }

    function setDappleDeposit(bool status) external onlyOwner {
        isDappleDeposit = status;
    }

    function deposit(
        address referrer,
        address referralContract,
        uint256 depositAmount,
        uint256 stakeType,
        bool isUsdt
    ) external whenNotPaused nonReentrant {        
        require(stakeType < 3, "D-1");
        address user = _msgSender();
        uint128 newTicket = currentTicket++;


        require(!isBlockedUsers[user], "B");
        if(referrerInfo[user] == address(0)) {
            if(referralContract == address(0)) {
                require(referrerInfo[referrer] != address(0), "D-2");
            }else {
                require(isReferralContract[referralContract] && referrer == address(0), "D-3");
                referrer = IDappleV1(referralContract).referrerInfo(user);
            }
            referrerInfo[user] = referrer;
        } else {
            require(referrerInfo[user] == referrer && referralContract == address(0), "D-4");
        }

        require(depositAmount >= feeInfo.minimumDeposit, "D-5");

        if(isUsdt) {
            usdt.safeTransferFrom(user,address(this),depositAmount);
        } else {
            require(isDappleDeposit, "D-6");
            dapp.safeTransferFrom(user,address(this),depositAmount);
            uint256 beforeBalance = usdtBalance();
            swapDappForUSDT(depositAmount);
            uint256 afterBalance = usdtBalance();
            depositAmount = afterBalance - beforeBalance;
        }

        uint256 stakeAmount = distributeUSDTCommission(user,depositAmount,feeInfo.dF,referralRC.dRC);    
            
        userInfo[newTicket] = userData({
            user: user,
            stakeAmount: stakeAmount,
            earnedAmount: 0,
            cR: feeInfo.cR,
            stakeTime: uint32(block.timestamp),
            unlockTime: uint32(block.timestamp) + durationInfo.stakeDuration[stakeType],
            claimTime: uint32(block.timestamp)
        });

        {
            ticketAdd(user,newTicket);
        }

        emit poolHarborEvent(
            newTicket,
            stakeAmount,
            1
        );
    }

    function reinvest(
        uint128 ticketId,
        uint256 stakeType
    ) external whenNotPaused nonReentrant {
        userData storage userStore = userInfo[ticketId];

        require(userTicketInfo[_msgSender()].contains(ticketId), "R-1");
        require(userStore.unlockTime <= uint32(block.timestamp), "R-2");
        require(stakeType < 3, "R-3");
        require(!isBlockedUsers[_msgSender()], "B");

        uint256 reward = getMaxReward(ticketId) - userStore.earnedAmount;
        userStore.earnedAmount = userStore.earnedAmount + (reward);

        {
            ticketRemove(userStore.user, ticketId);
        }

        uint256 amount = distributeUSDTCommission(userStore.user,userStore.stakeAmount + (reward),feeInfo.dF,referralRC.dRC);

        uint128 newTicket = currentTicket++;
        userInfo[newTicket] = userData({
            user: userStore.user,
            stakeAmount: amount,
            earnedAmount: 0,
            cR:feeInfo.cR,
            stakeTime: uint32(block.timestamp),
            unlockTime: uint32(block.timestamp) + durationInfo.stakeDuration[stakeType],
            claimTime: uint32(block.timestamp)
        });    

        {
            ticketAdd(userStore.user,newTicket);
        }  

        emit poolHarborEvent (
            newTicket,
            amount,
            4
        );
    }


    function claim(uint256 ticketId,uint256 amountOut,address rewardToken) external whenNotPaused nonReentrant{
        require(userTicketInfo[_msgSender()].contains(ticketId), "C-1");
        require(rewardTokenInfo[rewardToken], "C-2");
        require(!isBlockedUsers[_msgSender()], "B");
        
        userData storage userStore = userInfo[ticketId];

        uint256 reward = getMaxReward(ticketId) - userStore.earnedAmount;
        require(reward >= amountOut, "C-3");
        userStore.earnedAmount = userStore.earnedAmount + (amountOut);  
        userStore.claimTime = uint32(block.timestamp);
        amountOut = distributeUSDTCommission(userStore.user,amountOut,feeInfo.cF,referralRC.cRC);        
        shareReward(rewardToken,userStore.user,amountOut);
        
        emit poolHarborEvent (
            uint128(ticketId),
            amountOut,
            2
        );      
    }

    function withdraw(uint128 ticketId,uint256 amountOut,address rewardToken) external whenNotPaused nonReentrant {
        require(userTicketInfo[_msgSender()].contains(ticketId), "W-1");
        require(userInfo[ticketId].unlockTime <= uint32(block.timestamp), "W-2");
        require(rewardTokenInfo[rewardToken], "W-3");
        require(!isBlockedUsers[_msgSender()], "B");

        userData storage userStore = userInfo[ticketId];

        uint256 reward = getMaxReward(ticketId) - userStore.earnedAmount;          

        if(userStore.stakeAmount >= amountOut) {
            userStore.stakeAmount = userStore.stakeAmount - amountOut;
        }else if((userStore.stakeAmount + reward) >= amountOut){
            uint256 rewardOut = amountOut - userStore.stakeAmount;

            userStore.stakeAmount = 0;
            userStore.earnedAmount = userStore.earnedAmount + (rewardOut); 
        }else {
            revert("W-4"); 
        }
        
        userStore.claimTime = uint32(block.timestamp);
        amountOut = distributeUSDTCommission(userStore.user,amountOut,feeInfo.wF,referralRC.cRC); 
        shareReward(rewardToken,userStore.user,amountOut);
        
        if(userStore.stakeAmount == 0){
            ticketRemove(userStore.user, ticketId);
        }

        emit withdrawEvent (
            ticketId,
            amountOut,
            reward
        );
    }

    function booster(
        uint128 ticketId,
        uint256 depositAmount,
        address rewardToken,
        bool isUsdt) external whenNotPaused nonReentrant {
        userData storage userStore = userInfo[ticketId];

        address user  = _msgSender();
        uint32 currentTime = uint32(block.timestamp);

        require(userTicketInfo[user].contains(ticketId), "B-1");
        require(userStore.unlockTime > currentTime, "B-2");
        require(depositAmount >= feeInfo.minimumDeposit && depositAmount > 0, "B-3");
        require(rewardTokenInfo[rewardToken], "B-4");
        require(!isBlockedUsers[user], "B");

        if(isUsdt) {
            usdt.safeTransferFrom(user,address(this),depositAmount);
        } else {
            dapp.safeTransferFrom(user,address(this),depositAmount);
            uint256 beforeBalance = usdtBalance();
            swapDappForUSDT(depositAmount);
            uint256 afterBalance = usdtBalance();
            depositAmount = afterBalance - beforeBalance;
        }
        uint256 stakeAmount = distributeUSDTCommission(user,depositAmount,feeInfo.cF,referralRC.bRC);
        uint256 reward = getMaxReward(ticketId) - userStore.earnedAmount;

        userStore.stakeAmount = userStore.stakeAmount + (stakeAmount);
        userStore.earnedAmount = userStore.earnedAmount + (reward);  
        reward = distributeUSDTCommission(userStore.user,reward,feeInfo.bF,referralRC.cRC);        
        shareReward(rewardToken,userStore.user,reward);
       
        emit poolHarborEvent (
            ticketId,
            reward,
            3
        );
    }  

    function shareReward(address rewardToken,address user,uint256 reward) internal {
        if(rewardToken == address(usdt)){
            usdt.safeTransfer(user,reward);
        }else if(rewardToken == weth) {
            swapUsdtForEth(user,reward);
        }else {
            swapUsdtForRewardInRoute(user,rewardToken,reward);
        }
    }

    function swapDappForUSDT(uint256 dappAmount) internal {
        address[] memory path = new address[](3);
        path[0] = address(dapp);
        path[1] = weth;
        path[2] = address(usdt);

        swapTokenForToken(address(this),dappAmount,path);
    }  

    function swapUsdtForRewardInRoute(address wallet,address rewardAsset,uint256 usdtAmount) internal {
        address[] memory path = new address[](3);
        path[0] = address(usdt);
        path[1] = weth;
        path[2] = rewardAsset;

        swapTokenForToken(wallet,usdtAmount,path);
    }

    function swapTokenForToken(address wallet,uint256 amount,address[] memory path) internal {
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amount,
            0,
            path,
            wallet,
            uint32(block.timestamp) + (360)
        );
    }

    function swapUsdtForEth(address wallet,uint256 usdtAmount) internal {
        address[] memory path = new address[](2);
        path[0] = address(usdt);
        path[1] = weth;

        uniswapV2Router.swapExactTokensForETH(
            usdtAmount,
            0,
            path,
            wallet,
            uint32(block.timestamp) + (360)
        );
    }

    function getCurrentTime(uint32 unlockTime) internal view returns (uint32) {
        return (unlockTime < uint32(block.timestamp) ? unlockTime : uint32(block.timestamp));
    }

    function getYieldDuration() external view returns (uint256) {
        return durationInfo.rewardDuration[feeInfo.rewardBasics];
    }

    function getReward(uint256 ticketId) external view returns (uint256) {
        return (
            getMaxReward(ticketId) - userInfo[ticketId].earnedAmount
        );
    }

    function getMaxReward(uint256 ticketId) internal view returns (uint256) {
        uint32 currentTimestamp = getCurrentTime(userInfo[ticketId].unlockTime);

        return userInfo[ticketId].stakeAmount * (
                currentTimestamp - (userInfo[ticketId].stakeTime)) * (
                    userInfo[ticketId].cR) / (1e4 * durationInfo.rewardDuration[feeInfo.rewardBasics]);
    }

    function distributeUSDTCommission(address user, uint256 stakeAmount,uint16 adminFee, uint16[7] memory referrerShare) internal returns (uint256) {
        uint256 distributeAmount = stakeAmount * (adminFee) / (1e4);
        
        for(uint8 i; i<6; i++){
            if(commissionWalletShare[i] > 0) {
                usdt.safeTransfer(commissionWallets[i],distributeAmount * commissionWalletShare[i] / 1e4);
            }
        }

        emit adminCommissionEvent(distributeAmount);

        address referrer = user;
        for(uint8 i; i<7; i++) {
            referrer = referrerInfo[referrer];  

            if(referrerShare[i] > 0){
                uint256 refAmount = stakeAmount * (referrerShare[i]) / (1e4);
                distributeAmount = distributeAmount + (refAmount);
                usdt.safeTransfer(
                    referrer,
                    refAmount
                );   
                emit referrerCommissionEvent(
                    referrer,
                    i+1,
                    refAmount
                );   
            }
          
        }
        return stakeAmount - (distributeAmount);
    }

    function ticketAdd(address user,uint128 id) private {
        userTicketInfo[user].add(id);
    }

    function ticketRemove(address user,uint128 id) private {
        userTicketInfo[user].remove(id);
    }

    function getUserTicketLength(address account) external view returns (uint256) {
         return userTicketInfo[account].length();
    }
    
    function getUserTicketAt(address account,uint256 index) external view returns (uint256) {
        return userTicketInfo[account].at(index);
    }

    function getUserAllTickets(address account) external view returns (uint256[] memory) {
        return userTicketInfo[account].values();
    }
    
    function getUserTicketContains(address account,uint256 userTicket) external view returns (bool) {
        return userTicketInfo[account].contains(userTicket);
    }

    function viewFeeInfo() external view returns (fees memory info) {
        return feeInfo;
    }
}