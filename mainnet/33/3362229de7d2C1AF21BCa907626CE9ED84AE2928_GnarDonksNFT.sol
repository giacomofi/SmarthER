        );
        balance = 0;
        for (uint256 i = 1; i < ownerIndexTip; i++) {
            if (
                owners[i] > 0 &&
                address(uint160(owners[i] & WALLET_MASK)) == _owner
            ) {
                balance += owners[i] >> 224;
            }
        }
    }

