pragma solidity ^0.8.4;

interface IVault {
    function userInfo(address user) external view returns (uint256, bool);
}

contract DOGENSNFTV2 is ERC721A, ERC721AQueryable, Ownable, ReentrancyGuard {
    struct RoundData {
        uint256 rewardPerToken;
        bool isETH;
        uint256 upperLimit;
        uint256 rewardPoolBalance;
    }
    struct Receiver {
        address userAddress;
        uint256 quantity;
    }

    IERC20 public rewardToken;
    IVault public vault;

    // Public variables
    uint256 public MAX_MINTS = 1000;
    uint256 public MAX_SUPPLY = 1500;
    uint256 public mintRate = 0.25 ether;
    bool public saleEnable = false;
    bool public claimEnable = false;
    uint256 public roundId;
    uint256 public totalTokenClaimed;
    uint256 public totalEthClaimed;
    uint256 public maxTokenId;
    uint256 public min_erc_holding;
    uint16 public refFee = 10; //20%

    //mappings
    mapping(address => uint) public ref;
    mapping(uint256 => RoundData) public roundData;
    mapping(address => bool) public isExempted;

    // Mapping: token ID => last claimed round id
    mapping(uint256 => uint256) public lastClaimedRound;

    // Royalties address
    address public royaltyAddress = 0x363D4053cfB4C77597117f1c5f178F3668801109;

    // Treasure address
    address public treasureAddress;

    // Royalties basis points (percentage using 2 decimals - 1000 = 100, 500 = 50, 0 = 0) 10% artist 90% flipper
    uint256 private royaltyBasisPoints = 100; // 10%

    //NFT events
    event RoyaltyBasisPoints(uint256 indexed _royaltyBasisPoints);
    event MintedNft(uint256 indexed _quantity, address add);
    event SaleStatusChanged(bool newStatus, uint256 time);
    event MintPriceUpdated(uint256 newPrice, uint256 oldPrice, uint256 time);
    event RoyaltyAddressUpdated(
        address newAddress,
        address oldAddress,
        uint256 time
    );
    event UriUpdated(string newUri, uint256 time);
    event MintsPerWalletUpdated(
        uint256 newAmount,
        uint256 oldAmount,
        uint256 time
    );
    event MaxSupplyUpdated(uint256 newAmount, uint256 oldAmount, uint256 time);
    event RewardDeposited(
        uint256 amountPerToken,
        bool isEth,
        uint256 maxTokenId,
        uint256 totalAmount,
        uint256 roundId
    );
    event RoundCancelled(uint256 roundId);
    event ClaimStatusChanged(bool newStatus, uint256 time);
    event RewardTokenUpdated(address rewardToken, uint256 time);
    event VaultAddressUpdated(address vault, uint256 time);
    event MinErcHoldingUpdate(
        uint256 newAmount,
        uint256 oldAmount,
        uint256 time
    );
    event MaxTokenIdUpdated(
        uint256 newMaxTokenId,
        uint256 oldMaxTokenId,
        uint256 time
    );
    event RefFeeUpdated(uint256 newRefFee, uint256 oldRefFee, uint256 time);

    //Staking events
    event ClaimRewards(
        uint256 tokenId,
        uint256 _ethAmount,
        uint256 _tokenAmount,
        address recipient
    );

    string public baseURI =
        "ipfs://bafybeihdglamovbe56vuqo3uosoucjggwszsgzxcu7r3qwscmcpzhynhty/";

    address private signerAddress = 0x363D4053cfB4C77597117f1c5f178F3668801109;

    constructor() ERC721A("Dogens NFT V2", "yDogens2") {}

    //  NFT Contract section

    // Token Id start from 1
    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    function TotalBurned() public view returns (uint256) {
        return _totalBurned();
    }

    function next() public view returns (uint256) {
        return _nextTokenId();
    }

    function toggleSale(bool status) public onlyOwner {
        require(saleEnable != status);
        saleEnable = status;
        emit SaleStatusChanged(status, block.timestamp);
    }

    function getSigner(
        address _toAddress,
        uint _quantity,
        address _refAddrses,
        bytes memory signature
    ) internal pure returns (address) {
        bytes32 hash = keccak256(
            abi.encodePacked(_toAddress, _quantity, _refAddrses)
        );
        bytes32 message = ECDSA.toEthSignedMessageHash(hash);
        address signer = ECDSA.recover(message, signature);
        return signer;
    }

    function mint(uint256 quantity) external payable {
        require(saleEnable, "Sale is not Enabled");
        // _safeMint's second argument now takes in a quantity, not a tokenId.
        require(
            quantity + _numberMinted(_msgSender()) <= MAX_MINTS,
            "Exceeded the limit"
        );
        require(
            totalSupply() + quantity <= MAX_SUPPLY,
            "Not enough tokens left"
        );
        require(msg.value >= (mintRate * quantity), "Not enough ether sent");
        uint256 mintFee = msg.value;
        _safeMint(_msgSender(), quantity);
        payable(owner()).transfer(mintFee);
        emit MintedNft(quantity, _msgSender());
    }

    function mintbyref(
        address _toAddress,
        uint256 _quantity,
        address _refAddress,
        bytes memory _signature
    ) external payable {
        require(saleEnable, "Sale is not Enabled");
        // _safeMint's second argument now takes in a quantity, not a tokenId.
        require(
            _quantity + _numberMinted(_msgSender()) <= MAX_MINTS,
            "Exceeded the limit"
        );
        require(
            totalSupply() + _quantity <= MAX_SUPPLY,
            "Not enough tokens left"
        );
        require(msg.value >= (mintRate * _quantity), "Not enough ether sent");
        require(!isContract(_refAddress), "contracts not allowed");
        require(
            _refAddress != _toAddress && _toAddress == msg.sender,
            "Don't cheat"
        );
        require(
            getSigner(_toAddress, _quantity, _refAddress, _signature) ==
                signerAddress,
            "Don't cheat"
        );
        uint256 amountToRef = (msg.value * refFee) / 100;
        uint256 mintFee = msg.value - amountToRef;
        (bool success1, ) = payable(owner()).call{value: mintFee}("");
        require(success1, "eth transfer failed");
        (bool success2, ) = payable(_refAddress).call{value: amountToRef}("");
        require(success2, "eth transfer failed");

        _safeMint(_msgSender(), _quantity);
        ref[_refAddress] += _quantity;

        emit MintedNft(_quantity, _msgSender());
    }

    function giftmint(uint256 _quantity, address add) external onlyOwner {
        // _safeMint's second argument now takes in a quantity, not a tokenId.
        require(
            totalSupply() + _quantity <= MAX_SUPPLY,
            "Not enough tokens left"
        );
        _mint(add, _quantity);
        emit MintedNft(_quantity, add);
    }

    // Batch gift mint

    function BatchMintDrop(
        Receiver[] memory _addressesAndTokenQuantity
    ) external onlyOwner {
        for (uint i = 0; i < _addressesAndTokenQuantity.length; i++) {
            require(
                totalSupply() + _addressesAndTokenQuantity[i].quantity <=
                    MAX_SUPPLY,
                "Not enough tokens left"
            );

            _mint(
                _addressesAndTokenQuantity[i].userAddress,
                _addressesAndTokenQuantity[i].quantity
            );
            emit MintedNft(
                _addressesAndTokenQuantity[i].quantity,
                _addressesAndTokenQuantity[i].userAddress
            );
        }
    }

    // Emergency ETH withdrawal

    function emergencyWithdraw() external payable onlyOwner {
        (bool success, ) = payable(owner()).call{value: address(this).balance}(
            ""
        );
        require(success, "eth transfer failed");
    }

    // Emergency ERC20 withdrawal

    function rescueERC20(address tokenAdd, uint256 amount) external onlyOwner {
        // require(tokenAdd != address(this), "Cannot rescue self");
        require(
            IERC20(tokenAdd).balanceOf(address(this)) >= amount,
            "Insufficient ERC20 balance"
        );
        IERC20(tokenAdd).transfer(owner(), amount);
    }

    // returns base uri
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function setMintRate(uint256 _mintRate) public onlyOwner {
        uint256 oldPrice = mintRate;
        mintRate = _mintRate;
        emit MintPriceUpdated(_mintRate, oldPrice, block.timestamp);
    }

    // Set royalty  address
    function setRoyaltyAddress(address _address) external onlyOwner {
        require(_address != address(0), "zero address passed");
        address oldAddress = royaltyAddress;
        royaltyAddress = _address;
        emit RoyaltyAddressUpdated(_address, oldAddress, block.timestamp);
    }

    // Set base URI
    function setBaseURI(string memory _uri) external onlyOwner {
        baseURI = _uri;
        emit UriUpdated(_uri, block.timestamp);
    }

    // Set royalty basis points
    function setRoyaltyBasisPoints(uint256 _basisPoints) external onlyOwner {
        require(_basisPoints <= 1000, "more than 100%");
        royaltyBasisPoints = _basisPoints;
        emit RoyaltyBasisPoints(_basisPoints);
    }

    // Set new limit per wallet
    function changeMaxMintPerWallet(
        uint256 _max_mint_amount
    ) external onlyOwner {
        uint256 oldAmount = MAX_MINTS;
        MAX_MINTS = _max_mint_amount;
        emit MintsPerWalletUpdated(
            _max_mint_amount,
            oldAmount,
            block.timestamp
        );
    }

    // Set new max supply
    function changeMaxSupply(uint256 _newSupply) external onlyOwner {
        require(
            _newSupply > MAX_SUPPLY,
            "new supply cannot be less than current"
        );
        uint256 oldSupply = MAX_SUPPLY;
        MAX_SUPPLY = _newSupply;
        emit MaxSupplyUpdated(_newSupply, oldSupply, block.timestamp);
    }

    // TokenURI
    function tokenURI(
        uint256 tokenId
    ) public view virtual override returns (string memory) {
        require(_exists(tokenId), "Nonexistent token");

        return
            string(abi.encodePacked(_baseURI(), _toString(tokenId), ".json"));
    }

    // Staking contract logic

    function claimRewards(uint256 tokenId) public nonReentrant {
        claim(tokenId, msg.sender);
    }

    function batchClaimRewards(uint256[] memory tokenIds) public {
        require(
            tokenIds.length > 0 && tokenIds.length < MAX_MINTS,
            "max reached"
        );
        for (uint i = 0; i < tokenIds.length; i++) {
            if (lastClaimedRound[tokenIds[i]] < roundId) {
                claimRewards(tokenIds[i]);
            }
        }
    }

    function claimAndDepositReward(uint256 tokenId) public nonReentrant {
        claim(tokenId, treasureAddress);
    }

    function batchClaimAndDepositRewards(uint256[] memory tokenIds) public {
        require(
            tokenIds.length > 0 && tokenIds.length < MAX_MINTS,
            "max reached"
        );
        for (uint i = 0; i < tokenIds.length; i++) {
            if (lastClaimedRound[tokenIds[i]] < roundId) {
                claimAndDepositReward(tokenIds[i]);
            }
        }
    }

    function claim(uint256 tokenId, address rewardRecipient) internal {
        require(claimEnable, "claim suspended");
        //require(balanceOf(_msgSender()) > 0, "user nft balance = 0");
        require(_msgSender() == ownerOf(tokenId), "you not the token owner");
        (uint256 userBalance, bool canClaim) = vault.userInfo(_msgSender());
        if (!isExempted[_msgSender()]) {
            uint256 requiredAmount = min_erc_holding * balanceOf(_msgSender());
            require(userBalance >= requiredAmount, "not enough tokens locked");
            require(canClaim, "locking period less than minimum");
        }
        require(lastClaimedRound[tokenId] < roundId, "reward already claimed");
        (uint256 totalEthReward, uint256 totalTokenReward) = handleClaim(
            tokenId
        );
        require(totalEthReward > 0 || totalTokenReward > 0, "nothing to claim");
        lastClaimedRound[tokenId] = roundId;
        if (totalEthReward > 0) {
            unchecked {
                totalEthClaimed += totalEthReward;
            }
            _transferEth(rewardRecipient, totalEthReward);
        }
        if (totalTokenReward > 0) {
            unchecked {
                totalTokenClaimed += totalTokenReward;
            }
            rewardToken.transfer(rewardRecipient, totalTokenReward);
        }
        emit ClaimRewards(
            tokenId,
            totalEthReward,
            totalTokenReward,
            rewardRecipient
        );
    }

    function handleClaim(uint256 tokenId) internal returns (uint256, uint256) {
        uint256 totalEthReward;
        uint256 totalTokenReward;
        for (uint i = lastClaimedRound[tokenId]; i < roundId; i++) {
            if (tokenId <= roundData[i].upperLimit) {
                if (roundData[i].isETH) {
                    unchecked {
                        totalEthReward += roundData[i].rewardPerToken;
                        roundData[i].rewardPoolBalance -= roundData[i]
                            .rewardPerToken;
                    }
                } else {
                    unchecked {
                        totalTokenReward += roundData[i].rewardPerToken;
                        roundData[i].rewardPoolBalance -= roundData[i]
                            .rewardPerToken;
                    }
                }
            }
        }
        if (totalTokenReward > 0 || totalEthReward > 0) {
            return (totalEthReward, totalTokenReward);
        }
        return (0, 0);
    }

    function getUnclaimedAmount(
        uint256 tokenId
    ) public view returns (uint256, uint256) {
        if (tokenId == 0 || tokenId > MAX_SUPPLY) {
            return (0, 0);
        }
        uint256 totalEthReward;
        uint256 totalTokenReward;
        for (uint i = lastClaimedRound[tokenId]; i < roundId; i++) {
            if (tokenId <= roundData[i].upperLimit) {
                if (roundData[i].isETH) {
                    totalEthReward += roundData[i].rewardPerToken;
                } else {
                    totalTokenReward += roundData[i].rewardPerToken;
                }
            }
        }
        if (totalTokenReward > 0 || totalEthReward > 0) {
            return (totalEthReward, totalTokenReward);
        }
        return (0, 0);
    }

    function batchGetUnclaimedAmount(
        uint256[] memory tokenIds
    ) public view returns (uint256, uint256) {
        uint256 totalEthReward;
        uint256 totalTokenReward;
        for (uint i = 0; i < tokenIds.length; i++) {
            (uint256 eth_amount, uint256 token_amount) = getUnclaimedAmount(
                tokenIds[i]
            );
            totalEthReward += eth_amount;
            totalTokenReward += token_amount;
        }
        return (totalEthReward, totalTokenReward);
    }

    function depositRewardToken(uint256 amount) external onlyOwner {
        require(totalSupply() > 0, "totalSupply amount is 0");
        require(maxTokenId > 0, "max token id not set");
        uint256 decimals = 10 ** rewardToken.decimals();
        uint256 totalAmount = amount * decimals;
        rewardToken.transferFrom(msg.sender, address(this), totalAmount);
        uint256 amountPerToken = totalAmount / maxTokenId;
        roundData[roundId] = RoundData(
            amountPerToken,
            false,
            maxTokenId,
            totalAmount
        );
        emit RewardDeposited(
            amountPerToken,
            false,
            maxTokenId,
            totalAmount,
            roundId
        );
        roundId++;
    }

    function depositRewardEth() external payable onlyOwner {
        require(totalSupply() > 0, "totalSupply amount is 0");
        require(maxTokenId > 0, "max token id not set");
        _transferEth(address(this), msg.value);
        uint256 amountPerToken = msg.value / maxTokenId;
        roundData[roundId] = RoundData(
            amountPerToken,
            true,
            maxTokenId,
            msg.value
        );
        emit RewardDeposited(
            amountPerToken,
            true,
            maxTokenId,
            msg.value,
            roundId
        );
        roundId++;
    }

    function cancelRound(uint256 _roundId) external onlyOwner {
        require(_roundId < roundId, "wrong round passed");
        uint256 unclaimedReward = roundData[_roundId].rewardPoolBalance;
        if (roundData[_roundId].isETH) {
            _transferEth(owner(), unclaimedReward);
        } else {
            rewardToken.transfer(owner(), unclaimedReward);
        }
        delete roundData[_roundId];
        emit RoundCancelled(_roundId);
    }

    function toggleClaim(bool status) public onlyOwner {
        require(claimEnable != status, "wrong data passed");
        claimEnable = status;
        emit ClaimStatusChanged(status, block.timestamp);
    }

    function setRewardToken(address tokenAddress) external onlyOwner {
        require(tokenAddress != address(0), "zero address passed");
        rewardToken = IERC20(tokenAddress);
        emit RewardTokenUpdated(tokenAddress, block.timestamp);
    }

    function setVault(address vaultAddress) external onlyOwner {
        require(vaultAddress != address(0), "zero address passed");
        vault = IVault(vaultAddress);
        emit VaultAddressUpdated(vaultAddress, block.timestamp);
    }

    function setMinErcHolding(uint256 newAmount) external onlyOwner {
        uint256 decimals = 10 ** rewardToken.decimals();
        uint256 totalAmount = newAmount * decimals;
        uint256 oldAmount = min_erc_holding;
        min_erc_holding = totalAmount;
        emit MinErcHoldingUpdate(totalAmount, oldAmount, block.timestamp);
    }

    function flipExemptionStatus(address user) external onlyOwner {
        isExempted[user] = !isExempted[user];
    }

    function setTreasureAddress(address _newTreasure) external onlyOwner {
        treasureAddress = _newTreasure;
    }

    function setSigner(address _signer) external onlyOwner {
        signerAddress = _signer;
    }

    function setMaxTokenId(uint256 newMaxTokenId) external onlyOwner {
        require(
            newMaxTokenId > 0 && newMaxTokenId <= MAX_SUPPLY,
            "Out of range"
        );
        uint256 oldId = maxTokenId;
        maxTokenId = newMaxTokenId;
        emit MaxTokenIdUpdated(newMaxTokenId, oldId, block.timestamp);
    }

    function changeRefFee(uint16 _newRefFee) external onlyOwner {
        require(_newRefFee <= 100, "More than 100%");
        uint256 oldRefFee = refFee;
        refFee = _newRefFee;
        emit RefFeeUpdated(_newRefFee, oldRefFee, block.timestamp);
    }

    /// @notice payout function
    /// @dev care about non reentrant vulnerabilities
    function _transferEth(address to, uint256 amount) internal {
        (bool transferSuccess, ) = payable(to).call{value: amount}("");
        require(transferSuccess, "eth transfer failed");
    }

    function isContract(address account) internal view returns (bool) {
        return account.code.length > 0;
    }

    receive() external payable {}
}