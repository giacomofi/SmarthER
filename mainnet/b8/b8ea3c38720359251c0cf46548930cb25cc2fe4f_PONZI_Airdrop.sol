// File: contracts/Ponzi/PonziAirdrop/PonziAirdrop.sol


pragma solidity ^0.8.7;





struct TokenOwnership {
    // The address of the owner.
    address addr;
    // Keeps track of the start time of ownership with minimal overhead for tokenomics.
    uint64 startTimestamp;
    // Whether the token has been burned.
    bool burned;
}

interface IERC721A{
    function balanceOf(address owner) external view returns (uint256 balance);
    function ownerOf(uint256 tokenId) external view returns (address owner);
    function getOwnershipData(uint256 tokenId) external view returns (TokenOwnership memory);
}

contract PONZI_Airdrop is Ownable {
    using Strings for uint256;
    
    IERC721A public PONZI_NFT;
    IERC20 public PONZI_TOKEN;

    uint256 public ONETIME_REWARDS = 0.008 ether;
    uint256 public DAILY_REWARDS_PER_NFT = 3.73 ether;
    uint256 public NFT_HOLD_LIMIT_PERIOD = 1 days;

    mapping(address => bool) public MAP_ONETIME_AIRDROP;
    mapping(address => uint256) public MAP_DAILY_AIRDROP_TIMESTAMP;
    mapping(address => uint256) public MAP_TOTAL_AIRDROP_CLAIMED;
    
    uint256 public AIRDROP_START_TIMESTAMP;
    uint256 public SALE_STEP = 0; // 0 => NONE, 1 => START

    constructor(address _ponziNFT, address _ponziToken) {
        PONZI_NFT = IERC721A(_ponziNFT);
        PONZI_TOKEN = IERC20(_ponziToken);
    }

    function setPonziNFT(address _ponziNFT) external onlyOwner {
        PONZI_NFT = IERC721A(_ponziNFT);
    }
    function setPonziToken(address _ponziToken) external onlyOwner {
        PONZI_TOKEN = IERC20(_ponziToken);
    }
    function setOnetimeRewards(uint256 _onetimeRewards) external onlyOwner {
        ONETIME_REWARDS = _onetimeRewards;
    }
    function setDailyRewardsPerNFT(uint256 _dailyRewardsPerNFT) external onlyOwner {
        DAILY_REWARDS_PER_NFT = _dailyRewardsPerNFT;
    }
    function setNFTHoldLimitPeriod(uint256 _limitPeriod) external onlyOwner {
        NFT_HOLD_LIMIT_PERIOD = _limitPeriod;
    }
    function setSaleStep(uint256 _saleStep) external onlyOwner {
        SALE_STEP = _saleStep;
        if (SALE_STEP == 1) AIRDROP_START_TIMESTAMP = block.timestamp;
    }

    function getCurrentDailyRewards(address _owner, uint256[] memory tokenIds) public view returns (uint256) {
        if (SALE_STEP != 1) return 0;
        uint256 airdropTimestamp = MAP_DAILY_AIRDROP_TIMESTAMP[_owner];
        if (airdropTimestamp + 1 days > block.timestamp) return 0;
        if (AIRDROP_START_TIMESTAMP + 1 days > block.timestamp) return 0;
        
        uint256 currentDailyRewards = 0;
        for(uint256 i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            TokenOwnership memory ownerShip = PONZI_NFT.getOwnershipData(tokenId);
            if (ownerShip.addr == _owner && ownerShip.startTimestamp + NFT_HOLD_LIMIT_PERIOD <= block.timestamp) {
                if (AIRDROP_START_TIMESTAMP < airdropTimestamp) {
                    if (airdropTimestamp < ownerShip.startTimestamp) {
                        currentDailyRewards += (block.timestamp - ownerShip.startTimestamp) * DAILY_REWARDS_PER_NFT / 1 days;
                    } else {
                        currentDailyRewards += (block.timestamp - airdropTimestamp) * DAILY_REWARDS_PER_NFT / 1 days;
                    }
                } else {
                    if (AIRDROP_START_TIMESTAMP < ownerShip.startTimestamp) {
                        currentDailyRewards += (block.timestamp - ownerShip.startTimestamp) * DAILY_REWARDS_PER_NFT / 1 days;
                    } else {
                        currentDailyRewards += (block.timestamp - AIRDROP_START_TIMESTAMP) * DAILY_REWARDS_PER_NFT / 1 days;
                    }
                }
            }
        }
        return currentDailyRewards;
    }

    function getTotalPotentialRewards(address _owner, uint256[] memory tokenIds) public view returns (uint256) {
        uint256 airdropTimestamp = MAP_DAILY_AIRDROP_TIMESTAMP[_owner];
        uint256 airdropEndTimestamp = AIRDROP_START_TIMESTAMP + 365 days;
        uint256 potentialRewards = 0;
        for(uint256 i = 0; i < tokenIds.length; i++) {
            uint256 tokenId = tokenIds[i];
            TokenOwnership memory ownerShip = PONZI_NFT.getOwnershipData(tokenId);
            if (ownerShip.addr == _owner) {
                if (AIRDROP_START_TIMESTAMP < airdropTimestamp) {
                    if (airdropTimestamp < ownerShip.startTimestamp) {
                        potentialRewards += (airdropEndTimestamp - ownerShip.startTimestamp) * DAILY_REWARDS_PER_NFT / 1 days;
                    } else {
                        potentialRewards += (airdropEndTimestamp - airdropTimestamp) * DAILY_REWARDS_PER_NFT / 1 days;
                    }
                } else {
                    if (AIRDROP_START_TIMESTAMP < ownerShip.startTimestamp) {
                        potentialRewards += (airdropEndTimestamp - ownerShip.startTimestamp) * DAILY_REWARDS_PER_NFT / 1 days;
                    } else {
                        potentialRewards += (airdropEndTimestamp - AIRDROP_START_TIMESTAMP) * DAILY_REWARDS_PER_NFT / 1 days;
                    }
                }
            }
        }
        return MAP_TOTAL_AIRDROP_CLAIMED[_owner] + potentialRewards;
    }

    function claimAirdropOnetime() external {
        require(!MAP_ONETIME_AIRDROP[msg.sender], "Already Claimed Onetime Rewards");
        require(SALE_STEP == 1, "Airdrop is not opened");

        PONZI_TOKEN.transfer(msg.sender, ONETIME_REWARDS);
        MAP_ONETIME_AIRDROP[msg.sender] = true;
        MAP_TOTAL_AIRDROP_CLAIMED[msg.sender] += ONETIME_REWARDS;
    }

    function claimAirdropDaily(uint256[] memory tokenIds) external {
        uint256 currentDailyRewards = getCurrentDailyRewards(msg.sender, tokenIds);

        require(currentDailyRewards > 0, "No Rewards");
        require(SALE_STEP == 1, "Airdrop is not opened");

        PONZI_TOKEN.transfer(msg.sender, currentDailyRewards);
        MAP_DAILY_AIRDROP_TIMESTAMP[msg.sender] = block.timestamp;
        MAP_TOTAL_AIRDROP_CLAIMED[msg.sender] += currentDailyRewards;
    }

    function withdraw() external onlyOwner {
        uint256 curBalance = address(this).balance;
        payable(msg.sender).transfer(curBalance);
    }

    function withdrawToken() external onlyOwner {
        uint256 curBalance = PONZI_TOKEN.balanceOf(address(this));
        PONZI_TOKEN.transfer(msg.sender, curBalance);
    }
}