                                VARIABLES
    //////////////////////////////////////////////////////////////*/

    using LibBitmap for LibBitmap.Bitmap;

    LibBitmap.Bitmap bitmap;
    address private constant _DUDE_DEV_ADDRESS =
        0x7787CF7FA044f751AD055C92A4BE4EA6cF4D0851;
    address private constant _DUDE_COUNCIL_ADDRESS =
        0x56E7483D12F22f4759f8BdA80b9AfFEBC25D74EA;
    address private constant _DUDE_FOUNDERS_ADDRESS =
        0x4f24d74F78cB2F97a8B7824dd7CABC2767756455;
    uint256 private constant _PRIVATE_FACTORY_INDEX = 1;
    uint256 private constant _PUBLIC_FACTORY_INDEX = 2;
    uint256 private constant _DUDE_INDEX = 3;
    uint256 private constant _CHOSEN_DUDES = 33;

    uint256 public maxDudenessPerMint = 2; 
    uint256 public POWER_REQUIRED = 0.042069 ether;
    uint256 public FACTORY_LIMIT = 3333;
    bytes32 public dudeRoot;
    
    string private baseTokenURI;

    /*//////////////////////////////////////////////////////////////
                               CONSTRUCTOR
    //////////////////////////////////////////////////////////////*/

    constructor(
        string memory baseURI,
        bytes32 dudeRoot_
    ) ERC721A("thedudedudes", "DUDEDUDE") {
        baseTokenURI = baseURI;
        dudeRoot = dudeRoot_;
    }

    /*//////////////////////////////////////////////////////////////
                                MODIFIER
    //////////////////////////////////////////////////////////////*/

    modifier makeDude(uint256 dudesToMint) {
        if (!LibBitmap.get(bitmap, _DUDE_INDEX)) revert OutOfDudeJuice();

        unchecked {
            if (totalSupply() + dudesToMint > FACTORY_LIMIT)
                revert FactoryLimit();
            if (
                _numberMinted(msg.sender) + dudesToMint > maxDudenessPerMint
            ) revert UltimateDudeness();
            if (msg.value != POWER_REQUIRED * dudesToMint)
                revert NotEnoughPower();
        }
        _;
    }

    /*//////////////////////////////////////////////////////////////
                            MINT FUNCTIONS
    //////////////////////////////////////////////////////////////*/

    function privateDudeFactory(
        bytes32[] calldata dudeProof,
        uint256 dudesToMint
    ) external payable makeDude(dudesToMint) {
        if (!LibBitmap.get(bitmap, _PRIVATE_FACTORY_INDEX))
            revert PrivateFactoryClosed();
        if (LibBitmap.get(bitmap, _PUBLIC_FACTORY_INDEX))
            revert PublicFactoryOpened();
        if (!symbolOfTheDude(dudeProof, msg.sender)) revert NotDudeEnough();

        _mint(msg.sender, dudesToMint);
    }

    function publicDudeFactory(uint256 dudesToMint)
        external
        payable
        makeDude(dudesToMint)
    {
        if (LibBitmap.get(bitmap, _PRIVATE_FACTORY_INDEX))
            revert PrivateFactoryOpened();
        if (!LibBitmap.get(bitmap, _PUBLIC_FACTORY_INDEX))
            revert PublicFactoryClosed();

        _safeMint(msg.sender, dudesToMint);
    }

    /*//////////////////////////////////////////////////////////////
                            UI FUNCTIONS
    //////////////////////////////////////////////////////////////*/

    function dudesMinted(address owner) external view returns (uint256) {
        return _numberMinted(owner);
    }

    function getDudeState(uint256 index) external view returns (bool) {
        return LibBitmap.get(bitmap, index);
    }

    function verifyDude(bytes32[] calldata dudeProof, address sender) external view returns (bool) {
        bool proof = symbolOfTheDude(dudeProof, sender);
        return proof;
    }

    /*//////////////////////////////////////////////////////////////
                            INTERNAL FUNCTIONS
    //////////////////////////////////////////////////////////////*/

    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    function symbolOfTheDude(bytes32[] calldata dudeProof, address sender)
        internal
        view
        returns (bool)
    {
        bytes32 leaf = keccak256(abi.encodePacked(sender));
        bool proof = MerkleProofLib.verify(dudeProof, dudeRoot, leaf);
        return proof;
    }

    function _extract(address _address, uint256 _amount) private {
        (bool success, ) = _address.call{value: _amount}("");
        if (!success) revert ExtractFailed();
    }

    function _powerSplit(uint256 balance) private {
        _extract(_DUDE_DEV_ADDRESS, (balance * 12) / 100);
        _extract(_DUDE_COUNCIL_ADDRESS, (balance * 59) / 100);
        _extract(_DUDE_FOUNDERS_ADDRESS, (balance * 29) / 100);
    }

    /*//////////////////////////////////////////////////////////////
                            ADMIN FUNCTIONS
    //////////////////////////////////////////////////////////////*/

    function setFactoryLimit(uint256 _factoryLimit) external onlyOwner {
      FACTORY_LIMIT = _factoryLimit;
    }

    function setPowerRequired(uint256 _power) external onlyOwner {
        POWER_REQUIRED = _power;
    }

    function setDudeRoot(bytes32 dudeRoot_) external onlyOwner {
        dudeRoot = dudeRoot_; 
    } 

    function setMaxDudenessPerMint(uint256 _maxDudenessPerMint) external onlyOwner {
        maxDudenessPerMint = _maxDudenessPerMint; 
    } 

    function makeMarketingDudes() external onlyOwner {
        unchecked {
            if (totalSupply() + _CHOSEN_DUDES > FACTORY_LIMIT)
                revert FactoryLimit();
        }

        _mint(_DUDE_COUNCIL_ADDRESS, _CHOSEN_DUDES);
    }

    function giftDude(uint256 totalTokensToGift, address giftAddress)
        external
        onlyOwner
    {
        unchecked {
            if (totalSupply() + totalTokensToGift > FACTORY_LIMIT)
                revert FactoryLimit();
        }

        _mint(giftAddress, totalTokensToGift);
    }

    function extractPower(uint256 amount) external onlyOwner {
        if (amount > address(this).balance) revert TooMuchPower();
        _powerSplit(amount);
    }

    function extractPowerAll() external onlyOwner {
        uint256 balance = address(this).balance;
        if (balance == 0) revert NotEnoughPower();
        _extract(_DUDE_COUNCIL_ADDRESS, balance);
    }

    function setBaseURI(string memory baseURI) external onlyOwner {
        baseTokenURI = baseURI;
    }

    //Index 1 - Controls Private Factory,
    //Index 2 - Controls Public Factory,
    //Index 3 - Controls Dude Juice,
    function toogleDudeState(uint256 index) external onlyOwner returns (bool pauseState){
        pauseState = LibBitmap.toggle(bitmap, index);
    }

}