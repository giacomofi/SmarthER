    /* -------------------------------------------------------------------------- */
    function teamMint(uint256 quantity_, address to_) external onlyOwner {
        if (totalSupply() + quantity_ > _maxSupply) revert ErrExceedsSupplyCap();
        _mint(to_, quantity_);
    }

