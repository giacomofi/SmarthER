contract NFTPriceUSD {
  GetFloorPrice floorPrice;
  GetSolPrice solPrice;
  uint public USDThreshold; 
  uint public currFloor;
  uint public currSol;
  uint public NFTPriceInUSD;

  // Event to be broadcasted when price is updated via Chainlink
  event FloorPriceUpdated(uint time, uint amount);

  // Event to be broadcasted when we get SOL/USD
  event SOLUpdated(uint time, int price);

  // Event to be broadcasted when DeGods is below a certain USD price
  event DeGodsPriceLow(uint time, uint price);

  constructor(uint _USDThreshold) {
    USDThreshold = _USDThreshold;
  }

  function existingFloorBot(address _floorAddress) public {
    floorPrice = GetFloorPrice(_floorAddress);
  }

  function existingSolBot(address _solAddress) public {
    solPrice = GetSolPrice(_solAddress);
  }

  function testFloorBot() public view returns(uint) {
    return floorPrice.getCurrentFloor();
  }

  function updateFloor() public  {
    floorPrice.findDeGodsFloorPrice();
  }

  // To check if floor price was updated
  function getFloor() public {
    uint fl = floorPrice.currentPrice();
    emit FloorPriceUpdated(block.timestamp, fl);
    currFloor = fl;
  }

  function getSol() public {
    int sol = solPrice.getLatestPrice();
    emit SOLUpdated(block.timestamp, sol);
    currSol = uint(sol);
  }

  // If you want the most recent value of currFloor, first call getFloor(). Only call this function 
  // once the PriceUpdated event has been emitted. This is because there is a high off-chain latency 
  // among the Chainlink nodes before the floor price is actually updated in the contract. 
  function checkNFTPriceUSD() public {
    uint256 currFloorPrice = floorPrice.currentPrice();
    this.getSol();
    uint256 currSolPrice = currSol;
    NFTPriceInUSD = currFloorPrice * currSolPrice / (10**15); // cents
    if (NFTPriceInUSD <= USDThreshold) {
      emit DeGodsPriceLow(block.timestamp, NFTPriceInUSD);
    }
  }

}