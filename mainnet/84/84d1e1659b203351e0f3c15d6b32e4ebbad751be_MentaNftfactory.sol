pragma solidity ^0.8.2;

contract MentaNft is ERC721, ERC721URIStorage, Pausable,ReentrancyGuard{
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    IERC20 public token;
    using SignatureChecker for address;

    uint256 public ethPriceAmount;
    uint256 public tokenPriceAmount;
    uint256 public Limits;
    uint256[] public NftId; 
    address public signerAddress;
    address public Adminwallet;
    uint internal numTokens = 0;
    uint internal nonce = 0;
    uint[] internal indices;


    constructor(string memory _name,string memory _symbol,string memory _uri,uint256 _limit,uint256 _ethPriceamount,uint256 _tokenpriceamount,address _signerAddress,address _token,address owner) ERC721(_name, _symbol,_uri)  {
       Limits = _limit;
       token = IERC20(_token);
       signerAddress = _signerAddress;
       Adminwallet = owner;                            
       ethPriceAmount = _ethPriceamount;
       tokenPriceAmount = _tokenpriceamount;
       indices = new uint[](_limit);
    }
    mapping(bytes => bool) private signStatus;
    mapping(address => EnumerableSet.UintSet) private userIdInfo;

    modifier _onlyOwner() {
        require(Adminwallet == msg.sender,"Not the Owner");
        _;
    }

    function pause() public _onlyOwner {
        _pause();
    }

    function unpause() public _onlyOwner {
        _unpause();
    }

    function mintsRemaining() external view returns (uint) {
        return Limits.sub(numTokens);
    }

    function setLimit(uint _limit)public _onlyOwner{
       require(Limits != 0,"Invalid limit");
       Limits += _limit;
       indices = new uint[](_limit);
    }

    function updateTokenPriceamount(uint _priceamount)public _onlyOwner{
       require(_priceamount != 0,"Invalid priceamount");
       tokenPriceAmount = _priceamount;
    }

    function updateEthPriceamount(uint _priceamount)public _onlyOwner{
       require(_priceamount != 0,"Invalid priceamount");
       ethPriceAmount = _priceamount;
    }

    function randomIndex() internal returns (uint) {
        uint totalSize = Limits - numTokens;
        uint index = uint(keccak256(abi.encodePacked(nonce, msg.sender, block.difficulty, block.timestamp))) % totalSize;
        uint value = 0;
        if (indices[index] != 0) {
            value = indices[index];
        } else {
            value = index;
        }

        // Move last value to selected position
        if (indices[totalSize - 1] == 0) {
            // Array position not initialized, so use position
            indices[index] = totalSize - 1;
        } else {
            // Array position holds a value so use that
            indices[index] = indices[totalSize - 1];
        }
        nonce++;
        // Don't allow a zero index, start counting at 1
        return value.add(1);
    }

    function safeMint(string memory uri,uint256 _amount,bytes memory signature,uint256 deadline,uint256 _flag) external payable whenNotPaused nonReentrant {
       require(numTokens < Limits, "Token limit reached.");
       require(_flag < 2,"Invalid flag");
       require(!signStatus[signature], "already sign used");
       address user = _msgSender();
        uint id = randomIndex();
        numTokens = numTokens + 1;
      
     
       bytes32 hash_ = keccak256(abi.encodePacked(
            address(this),
            user,
            _amount,
            deadline
        ));

       require(signCheck(toSignedMessageHash(hash_),signature), "Sign Error");

         if (_flag == 0) {
          require((_amount != 0 && _amount == tokenPriceAmount), "Invalidamount");
          token.safeTransferFrom(user,Adminwallet,_amount);
        }

        else {
           require((_amount == 0 && msg.value == ethPriceAmount), "Invalidamount");
           require(payable(Adminwallet).send(msg.value),"send failed");
           
        }

        _safeMint(user, id);
        _setTokenURI(id, uri);
        signStatus[signature] = true;
        userIdInfo[msg.sender].add(id);
        NftId.push(id);
    }

    function adminMint(address to, string memory uri)
        public
        _onlyOwner
    {
        require(to != address(0) && bytes(uri).length > 0,"To address can't be zero address or uri must not be empty!!!");
        require(numTokens < Limits, "Token limit reached.");
        uint id = randomIndex();
        numTokens = numTokens + 1;
      
        _safeMint(to, id);
        _setTokenURI(id, uri);
        userIdInfo[to].add(id);
        NftId.push(id);
        
    }


    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override(ERC721)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721, ERC721URIStorage)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }


    function recover(bytes32 hash,uint8 v,bytes32 r,bytes32 s) internal pure returns (address) {
        return ecrecover(hash, v, r, s);
    }
      
    function toSignedMessageHash(bytes32 hash) public pure returns (bytes32) {
        return keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", hash));
    }

    function signCheck(bytes32 hash,bytes memory signature) public view returns (bool) {
        return signerAddress.isValidSignatureNow(hash,signature);
    }


    function Salenft() public view returns (uint256) {
        return NftId.length;
    }

    function userIdLength(address account) public view returns (uint256) {
        return userIdInfo[account].length();
    }

    function useIdrAt(address account,uint256 index) public view returns (uint256) {
        return userIdInfo[account].at(index);
    }

    function userTotalIds(address account) public view returns (uint256[] memory) {
        return userIdInfo[account].values();
    }


}
contract MentaNftfactory is Ownable{
    address private owneraddress;
    MentaNft private _MentaNft;
    using SafeERC20 for IERC20;
    IERC20 public Token;
    address private signerAddress;
    uint public collectioncount;

    struct collectionDetails{
        uint id;
        address meta721;
        string name;
        string symbol;
        string uri;
        uint256 ethPriceamount;
        uint256 tokenpriceamount;
        
    }

    mapping(uint256 => collectionDetails) public collections;
    event SignerAddressUpdate(address signerAddress);
    event create(uint indexed _id, address indexed _nft721Address);

    constructor(address _signerAddress,address _tokenaddress){
        owneraddress = _owner;
        signerAddress = _signerAddress; 
        Token = IERC20(_tokenaddress);
    }  

    function updateTokenaddress(address _token) public onlyOwner {
        require(_token != address(0),"Token must be 0");
        Token = IERC20(_token);
    }
                         
    function signerAddressUpdate(address newSigner) public onlyOwner {
        require(newSigner != address(0),"Token must be 0");
        signerAddress = newSigner;
        emit SignerAddressUpdate(signerAddress);
    }

    function createCollection721(string memory _name ,string memory _symbol,string memory _uri,uint256 _limit,uint256 _ethPriceamount,uint256 _tokenpriceamount)onlyOwner external{
        require(bytes(_name).length > 0, "MentaNft721 : name must not be empty");
        require(bytes(_symbol).length > 0, "MentaNft721 : symbol must not be empty");
        address nftaddress =collections721(_name,_symbol,_uri,_limit,_ethPriceamount,_tokenpriceamount);
        collectioncount++;
        collectionDetails storage collection = collections[collectioncount];
        collection.id = collectioncount;
        collection.meta721 = nftaddress;
        collection.name = _name;
        collection.symbol = _symbol;
        collection.uri = _uri;
        collection.ethPriceamount= _ethPriceamount;
        collection.tokenpriceamount= _tokenpriceamount;

        emit create(collectioncount,address(nftaddress));
       
    }

    function collections721(string memory _name,string memory _symbol,string memory _uri,uint256 _limit,uint256  _ethPriceamount,uint256 _tokenpriceamount)private returns(address){
          _MentaNft = new MentaNft(_name,_symbol,_uri,_limit,_ethPriceamount,_tokenpriceamount,signerAddress,address(Token),owneraddress);
           return address(_MentaNft);
    }

}