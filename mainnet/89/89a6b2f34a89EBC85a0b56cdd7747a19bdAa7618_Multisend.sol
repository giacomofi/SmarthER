/**
 * @dev: @brougkr
 */
pragma solidity 0.8.17;
contract Multisend
{
    mapping(address=>bool) public Admin;
    mapping(address=>bool) public Owner;

    constructor()
    { 
        Admin[0xe06F5FAE754e81Bc050215fF89B03d9e9FF20700] = true; 
        Owner[0xe06F5FAE754e81Bc050215fF89B03d9e9FF20700] = true;
    }

    /**
     * @dev Batch Sends NFTs
     */
    function BatchSendNFTs (
        address ContractAddress, 
        address Operator, 
        address[] calldata Addresses, 
        uint[] calldata TokenIDs
    ) external onlyAdmin {
        INFT NFT = INFT(ContractAddress);
        for(uint x; x < Addresses.length;) 
        { 
            NFT.transferFrom(Operator, Addresses[x], TokenIDs[x]); 
            unchecked { x++; }
        }
    }   

    /**
     * @dev Executes Batch Transactions
     */
    function BatchCall (
        address[] calldata Targets, 
        uint[] calldata Values, 
        bytes[] calldata Datas
    ) external onlyOwner {
        for (uint x; x < Targets.length;) 
        {
            (bool Success,) = Targets[x].call {value: (Values[x]) }(Datas[x]);
            if (!Success) { revert("transaction failed"); }
            unchecked { x++; }
        }
    }

    /**
     * @dev Executes Batch Payable Transactions
     */
    function BatchCallPayable (
        address[] calldata Targets, 
        uint[] calldata Values, 
        bytes[] calldata Datas
    ) external payable onlyOwner {
        for (uint x; x < Targets.length;) 
        {
            (bool Success,) = Targets[x].call {value: (Values[x]) }(Datas[x]);
            if (!Success) { revert("transaction failed"); }
            unchecked { x++; }
        }
    }

    /**
     * @dev Flips Admin State
     */
    function _FlipAdminState(address Wallet) external onlyOwner { Admin[Wallet] = !Admin[Wallet]; }

    /**
     * @dev onlyAdmin
     */
    modifier onlyAdmin
    {
        require(Admin[msg.sender]);
        _;
    }

    /**
     * @dev onlyOwner
     */
    modifier onlyOwner
    {
        require(Owner[msg.sender]);
        _;
    }
}

/**
 * @dev Helper Interface For NFT Transferring
 */
interface INFT { function transferFrom(address From, address To, uint TokenID) external; }