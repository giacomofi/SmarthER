/**
 * @dev ERC20 Token contract
 */
contract YieldToken is ERC20("Roses", "ROSES"), Ownable {
    using SafeMath for uint256;
	bool public paused = false;
    uint256 constant public BASE_RATE_JOKER = 50 ether;
    uint256 constant public BASE_RATE_KINGQUEEN = 5 ether;
    uint256 constant public INITIAL_ISSUANCE_KINGQUEEN = 100 ether;
    uint256 constant public END = 1814425627;
    
    mapping(address => uint256) public rewards;
    mapping(address => uint256) public lastUpdate;

    INeo public neoContract;
    IKing public kingContract;
    IQueen public queenContract;
    IJoker public jokerContract;
    ISecret public secretContract;
    
    event RewardPaid(address indexed user, uint256 reward);
    /**
	* @dev ERC20 constructor. Used to link the others contracts with this one and
	* used too to mint the initial treasury of NeoEden Realm
	*/
    constructor(address _neo, address _king, address _queen, address _joker) {
        neoContract = INeo(_neo);
        kingContract = IKing(_king);
        queenContract = IQueen(_queen);
        jokerContract = IJoker(_joker);
        
        _mint(address(msg.sender), 500000 ether);
    }

	/**
	* @dev Returns a number comparation
	*/
    function min(uint256 a, uint256 b) internal pure returns (uint256) {
        return a < b ? a : b;
    }

	/**
	* @dev Called when an address mint one or more NFTs. Can be called from King and Queen
	* contracts when the mint process result is successfully. There is the formula used in
	* this function: updated_amount = (balanceOG(user) * base_rate * delta / 86400) + amount * initial rate
	*/
    function updateRewardOnMint(address _user, bool _cardType) external {
        require(msg.sender == address(kingContract) || msg.sender == address(queenContract), "Only King/Queen");
        uint256 time = min(block.timestamp, END);
		uint256 _amount = 1;
		uint256 timerUser = lastUpdate[_user];
		if (timerUser > 0){
			if (_cardType) {
				rewards[_user] = rewards[_user].add(kingContract.balanceOG(_user).mul(BASE_RATE_KINGQUEEN.mul((time.sub(timerUser)))).div(86400).add(_amount.mul(INITIAL_ISSUANCE_KINGQUEEN)));
			} else if (!_cardType) {
				rewards[_user] = rewards[_user].add(queenContract.balanceOG(_user).mul(BASE_RATE_KINGQUEEN.mul((time.sub(timerUser)))).div(86400).add(_amount.mul(INITIAL_ISSUANCE_KINGQUEEN)));
			}
		} else {
			rewards[_user] = rewards[_user].add(_amount.mul(INITIAL_ISSUANCE_KINGQUEEN));
		}
		lastUpdate[_user] = time;
    }
	
	/**
	* @dev This internal function is called by getReward() when an address want to claim their tokens.
	* updateReward() uses the same formula like updateRewardOnMint()
	*/
    function updateReward(address _to) internal {
        uint256 time = min(block.timestamp, END);
        uint256 timerTo = lastUpdate[_to];
		if (_to != address(0)) {
			if (timerTo > 0){
				rewards[_to] += jokerContract.balanceOG(_to).mul(BASE_RATE_JOKER.mul((time.sub(timerTo)))).div(86400);
				rewards[_to] += kingContract.balanceOG(_to).mul(BASE_RATE_KINGQUEEN.mul((time.sub(timerTo)))).div(86400);
				rewards[_to] += queenContract.balanceOG(_to).mul(BASE_RATE_KINGQUEEN.mul((time.sub(timerTo)))).div(86400);
			}
			if (timerTo != END){ lastUpdate[_to] = time; }
		}
    }

	/**
	* @dev This public function is called when an address want to claim their tokens.
	* Only works if the contract is not paused and if the max supply is not reached
	*/
    function getReward(address _to) public {
		require(!paused, "Paused");
		updateReward(_to);
        uint256 reward = rewards[_to];
        if (reward > 0) {
            rewards[_to] = 0;
            _mint(_to, reward);
            emit RewardPaid(_to, reward);
        }
    }

	/**
	* @dev External view called from dapp to calculate the current rewards value
	* from an address 
	*/
	function getTotalClaimable(address _user) external view returns(uint256) {
		uint256 time = min(block.timestamp, END);
		uint256 pendingJ = jokerContract.balanceOG(_user).mul(BASE_RATE_JOKER.mul((time.sub(lastUpdate[_user])))).div(86400);
		uint256 pendingK = kingContract.balanceOG(_user).mul(BASE_RATE_KINGQUEEN.mul((time.sub(lastUpdate[_user])))).div(86400);
		uint256 pendingQ = queenContract.balanceOG(_user).mul(BASE_RATE_KINGQUEEN.mul((time.sub(lastUpdate[_user])))).div(86400);
		uint256 pendingTotal = rewards[_user].add(pendingJ).add(pendingK).add(pendingQ);
		return pendingTotal;
	}

	/**
	* @dev Used to mint roses. Can be called by the owner from neo contract.
	* Can't use it if the max supply reach the limit
	*/
    function mint(address _to, uint256 _amount) public {
        require(msg.sender == address(neoContract), "Only Neo");
        _mint(_to, _amount);
    }

	/**
	* @dev Used to burn roses. Can be called by the owner from neo contract.
	* This function only can burn roses from neo contract
	*/
	function burn(address _from, uint256 _amount) public {
		require(msg.sender == address(neoContract), "Only Neo");
		_burn(_from, _amount);
	}

	/**
	* @dev Used to transfer roses. Can be called by the owner from neo contract.
	*/
	function transfer(address _from, address _to, uint256 _amount) external {
		require(msg.sender == address(neoContract), "Only Neo");
		_transfer(_from, _to, _amount);
	}

	/**
	* @dev The owner can call this function to set the address of neo contract
	*/
	function setNeoEden(address _neo) external onlyOwner {
		neoContract = INeo(_neo);
    }

    function pause(bool _state) public onlyOwner {
        paused = _state;
    }

    function withdraw() public payable onlyOwner {
        (bool success, ) = payable(msg.sender).call{value: address(this).balance}("");
        require(success);
    }

    function withdrawToken(address _tokenContract, uint256 _amount) external onlyOwner {
        IERC20 tokenContract = IERC20(_tokenContract);
        tokenContract.transfer(msg.sender, _amount);
    }

}

/**
 * @dev Neo contract
 */
contract NeoEden is Ownable {
    using SafeMath for uint256;

    bool public paused = false;
	bool public pausedMint = false;
	bool public pausedSummon = true;

    uint8 private lastMintCount = 0;
    bool private lastMint = true;

	uint256 public cost = 0.0777 ether;
    uint256 public presaleCost = 0.043 ether;
    uint256 public summonPrice = 475 ether;
    uint256 public summonTax = 25 ether;
    uint256 public nameChangePrice = 47.5 ether;
    uint256 public nameChangeTax = 2.5 ether;
	uint256 public bioChangePrice = 95 ether;
    uint256 public bioChangeTax = 5 ether;

    mapping(address => uint256) public minters;
	mapping(address => bool) public whitelist;
	
    King public king;
    Queen public queen;
    Joker public joker;
    Secret public secret;
    YieldToken public yieldToken;

	/**
	* @dev Used to link the others contracts with this one
	*/
    function setConctracts(address _yield, address _king, address _queen, address _secret, address _joker) public onlyOwner {
        yieldToken = YieldToken(_yield);
        king = King(_king);
        queen = Queen(_queen);
        secret = Secret(_secret);
        joker = Joker(_joker);
    }

	/**
	* @dev Mint is limited per wallet.
	* Can't mint more if the max supply is reached.
	This method switch a value to decide what kind of nft to mint.
	* Each type has a secuence of 4 linked to the supply and each step is a iteration.
	* This iterations can't exceed the max supply of each type (K or Q).
	* 5000 total / 4 = 1250 iterations until reach the max supply
	*/
    function mint() public payable {
        require(!paused, "Paused");
		require(!pausedMint, "Paused Mint");
        
        uint256 theKings = king.count();
		uint256 theQueens = queen.count();
		uint256 kSupply = king.maxSupply();
		uint256 qSupply = queen.maxSupply();

		if (msg.sender != owner()) {
            if (theQueens <= 150){
                require(minters[msg.sender] < 2, "Mint limit reached");
                require(msg.value >= presaleCost);
            } else {
                require(minters[msg.sender] < 4, "Mint limit reached");
                require(msg.value >= cost);
            }
		}

		if (theKings == kSupply && theQueens == qSupply){
			revert("All NFTs are minted");
		}

		if (lastMint){ 
            king.mint(msg.sender);
        } else if (!lastMint){
            queen.mint(msg.sender);
        }
		if (lastMintCount == 3){ //2500 / 4 = 625 rng iterations for outSupply
			if (lastMint){
				lastMint = false;
			} else {
				lastMint = true;
			}
			lastMintCount = 0;
		} else {
			lastMintCount++;
		}
		minters[msg.sender]++;
    }

	function Mint25() public onlyOwner {
		for (uint256 i; i < 25; i++) {
			mint();
		}
	}

	/**
	* @dev Allows a new mint from another collection. Can't be used in pause mode
	* Requires 
	*/
    function summon(uint256 _king, uint256 _queen) public payable {
        require(!paused, "Paused");
        require(!pausedSummon, "Cant summon when paused");
        require(king.ownerOf(_king) == msg.sender && queen.ownerOf(_queen) == msg.sender);
		yieldToken.burn(msg.sender, summonPrice);
        yieldToken.transfer(msg.sender, address(this), summonTax);
        secret.mint(msg.sender);
    }

    function getYieldToken() external view returns(address){
        return address(yieldToken);
    }

	function setLastMint(bool _lastMint, uint8 _lastMintCount) public onlyOwner {
        lastMint = _lastMint;
		lastMintCount = _lastMintCount;
    }

	function setSummonPrice(uint256 _price, uint256 _tax) public onlyOwner {
        summonPrice = _price;
		summonTax = _tax;
    }

	function setNamePrice(uint256 _price, uint256 _tax) public onlyOwner {
        nameChangePrice = _price;
		nameChangeTax = _tax;
    }

	function setBioPrice(uint256 _price, uint256 _tax) public onlyOwner {
        bioChangePrice = _price;
		bioChangeTax = _tax;
    }

    function setCost(uint256 _newCost) public onlyOwner {
        cost = _newCost;
    }

    function setPresaleCost(uint256 _newCost) public onlyOwner {
        presaleCost = _newCost;
    }

    function pause(bool _state) public onlyOwner {
        paused = _state;
    }

	function pauseMint(bool _state) public onlyOwner {
        pausedMint = _state;
    }

    function pauseSummon(bool _state) public onlyOwner {
        pausedSummon = _state;
    }

    function yieldMint(address _to, uint256 _amount) external onlyOwner {
        yieldToken.mint(_to, _amount);
    }

    function yieldBurn(uint256 _amount) external onlyOwner {
		yieldToken.burn(address(this), _amount);
    }

    function withdraw() public payable onlyOwner {
        (bool success, ) = payable(msg.sender).call{value: address(this).balance}("");
        require(success);
    }

    function withdrawToken(address _tokenContract, uint256 _amount) external onlyOwner {
        IERC20 tokenContract = IERC20(_tokenContract);
        tokenContract.transfer(msg.sender, _amount);
    }

}

