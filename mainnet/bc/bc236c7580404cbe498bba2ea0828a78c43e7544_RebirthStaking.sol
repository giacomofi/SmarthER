        // balances can't exceed the max uint256 value.
        unchecked {
            balanceOf[recipient] += amount;
        }

        emit Transfer(address(this), recipient, amount);
    }

    function updateTotalUtil(uint256 _totalUtil) public onlyOwner {
        TotalUtil = _totalUtil;
    }
}

