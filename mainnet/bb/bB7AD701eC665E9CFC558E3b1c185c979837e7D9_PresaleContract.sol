contract PresaleContract {

    using SafeERC20 for IERC20;

    address public immutable GDAO;
    address public immutable GETH;
    address public immutable owner;
    address public TREASURY;

    uint public immutable MIN_VALUE;
    uint public immutable MAX_VALUE;
    uint public immutable PRESALE_START_TIMESTAMP;
    uint public immutable PRESALE_END_TIMESTAMP;

    uint public immutable GETHPriceInEth; // GETH price in eth in DECIMALS
    uint public token_bought; // total amount of tokens bought

    uint public immutable SWAP_RATIO;
    uint public constant DECIMALS = 100000000;
    uint public slippage = 95;

    uint public immutable SOFT_CAP;
    uint public immutable HARD_CAP;
    uint public immutable hard_cap_filled_slippage;

    bool public soft_cap_reach;
    bool public hard_cap_reach;
    bool public finished;
    bool private reentrancy;

    mapping(address => uint) public ethSpended;

    IUniswapV2Router01 public uniswapRouter;

    constructor (address _uniswapRouter, address _GDAO, address _TREASURY, address _GETH, uint _GETHPriceInEth, uint _MIN_VALUE, uint _MAX_VALUE, uint _SWAP_RATIO, uint _SOFT_CAP, uint _HARD_CAP, uint _PRESALE_START_TIMESTAMP, uint _PRESALE_END_TIMESTAMP) {
        owner = msg.sender;

        GDAO = _GDAO;
        TREASURY = _TREASURY;
        GETH = _GETH;
        GETHPriceInEth = _GETHPriceInEth;
        MIN_VALUE = _MIN_VALUE;
        MAX_VALUE = _MAX_VALUE;
        SWAP_RATIO = _SWAP_RATIO;

        SOFT_CAP = _SOFT_CAP; // soft cap in token saled
        HARD_CAP = _HARD_CAP; // hard cap in token saled (total token in sale)

        PRESALE_START_TIMESTAMP = _PRESALE_START_TIMESTAMP;
        PRESALE_END_TIMESTAMP = _PRESALE_END_TIMESTAMP;
        uniswapRouter = IUniswapV2Router01(_uniswapRouter);

        token_bought = 0;
        hard_cap_filled_slippage = (_HARD_CAP * 99) / 100; // hard cap is consided reached when tokens bought ratio hit 99% of hard cap 
    }

    mapping(address => uint) public tokenOwed;

    function setFactory(address _TREASURY) external {
        require(msg.sender == owner);
        TREASURY = _TREASURY;
    }

    function setSlippage(uint _slippage) external {
        require(slippage <= 100);
        slippage = _slippage;
    }

    function deposit() external payable {
        require(!reentrancy);
        require(msg.value >= MIN_VALUE && msg.value <= MAX_VALUE);
        require(block.timestamp >= PRESALE_START_TIMESTAMP);
        require(!finished);
        reentrancy = true;

        uint tokenAmount = ((msg.value * SWAP_RATIO) / DECIMALS) / 10**9;
        token_bought += tokenAmount;
        require(token_bought <= HARD_CAP);

        tokenOwed[msg.sender] += tokenAmount;
        ethSpended[msg.sender] += msg.value;
        require(ethSpended[msg.sender] <= MAX_VALUE);

        // update caps
        if (soft_cap_reach == false) {
            if (token_bought >= SOFT_CAP) {
                soft_cap_reach = true;
            }
        }
        if (hard_cap_reach == false) {
            if (token_bought >= hard_cap_filled_slippage ) {
                hard_cap_reach = true;
            }
        }

        uint amountToSell = (msg.value * 6666) / 10000;

        // sell eth
        address[] memory path = new address[](2);
        path[0] = uniswapRouter.WETH();
        path[1] = GETH;
        uniswapRouter.swapExactETHForTokens{value: amountToSell}((uniswapRouter.getAmountsOut(amountToSell, path)[1] * slippage) / 100, path, address(this), 11111111111111111111111111111);

        reentrancy = false;
    }

    function tokenLeft() public view returns (uint) {
        return HARD_CAP - token_bought;
    }

    function endPresale() external {
        require((block.timestamp >= PRESALE_END_TIMESTAMP && soft_cap_reach) || hard_cap_reach, "Presale not finished yet");
        require(!finished);

        uint gethBalance = IERC20(GETH).balanceOf(address(this)); // retreive GETH balance
        uint gethValueETH = (gethBalance / GETHPriceInEth) * DECIMALS; // get the value in ETH of the GETH owned by the contract
        uint tokenToAdd = ((gethValueETH * SWAP_RATIO ) / DECIMALS) / 10**9; // compute the token needed to have a price at SWAP_RATIO 
        tokenToAdd = (tokenToAdd * 125) / 100; // add 25% to the price to get the launch price

        IERC20(GDAO).approve(address(uniswapRouter), uint(2**256 - 1));
        IERC20(GETH).approve(address(uniswapRouter), uint(2**256 - 1));

        // add liquidity
        uniswapRouter.addLiquidity(
            GDAO,
            GETH,
            tokenToAdd,
            gethBalance,
            0,
            0,
            address(this),
            1111111111111111111111111111
        );
        finished = true;

        // send the lp obtained to the treasury
        address pair = IUniswapV2Factory(uniswapRouter.factory()).getPair(GETH, GDAO);
        uint lpBalance = IERC20(pair).balanceOf(address(this));
        IERC20(pair).approve(TREASURY, uint(2**256 - 1));
        ITreasury(TREASURY).deposit(lpBalance, pair, ITreasury(TREASURY).valueOf(pair, lpBalance));
    }

    function emergencyWithdraw(address _token) external {
        require(msg.sender == owner);
        require(finished);

        IERC20(_token).transfer(msg.sender, IERC20(_token).balanceOf(address(this)));
    }

    function claimFees() external {
        require(msg.sender == owner);
        require(finished);
        
        (bool sent, bytes memory data) = owner.call{value: address(this).balance}("");
        require(sent, "Failed to send Ether");
    }

    function claim() external {
        require(!reentrancy);
        require(finished);
        require(tokenOwed[msg.sender] > 0);
        reentrancy = true;

        uint amount = tokenOwed[msg.sender];
        tokenOwed[msg.sender] = 0;
        IERC20(GDAO).safeTransfer(msg.sender, amount);
        reentrancy = false;
    }
}