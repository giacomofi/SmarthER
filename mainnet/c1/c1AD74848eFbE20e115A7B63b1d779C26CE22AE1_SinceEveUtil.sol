contract SinceEveUtil is Ownable {
    using Strings for uint256;
    using SafeMath for uint256;

    uint256 public price_Public = 0.08 ether;
    function setPrice_Public(uint256 number) public onlyOwner() {
        price_Public = number;
    }

    bool public status = true;
    function switch_status(bool s) public onlyOwner {
        status = s;
    }

    uint256 public mint_perAdd = 10;
    function setMaxPerAdd(uint256 number) public onlyOwner {
        mint_perAdd = number;
    }

    address public sinceEve = 0xd9145CCE52D386f254917e481eB44e9943F39138;
    function setSinceEve(address a) public onlyOwner {
        sinceEve = a;
    }

    constructor() {}

    mapping(address => uint) public numberMinted;
    
    function mint(uint256 quantity,address _a) public payable {
        require(status, "Minting not active!!");
        require(numberMinted[_a].add(quantity) <= mint_perAdd && quantity > 0, "Invalid mint quantity!!");
        require(msg.value >= quantity.mul(price_Public), "Insufficient eth sent for mint!!");

        IERC721A(sinceEve).giveaway(_a, quantity);
    }

    function restoreOWnership(address _a) public onlyOwner {
        Ownable(sinceEve).transferOwnership(_a);
    }

    address public crossmintAddress = 0xdAb1a1854214684acE522439684a145E62505233;
    function setCrossmintAddress(address _crossmintAddress) public onlyOwner {
        crossmintAddress = _crossmintAddress;
    }

    function crossmint(address _to, uint256 quantity) public payable {
        require(status, "Minting not active!!");
        require(quantity > 0, "Invalid mint quantity!!");
        require(msg.value >= quantity.mul(price_Public), "Insufficient eth sent for mint!!");       
        require(msg.sender == crossmintAddress, "This function is for Crossmint only.");

        IERC721A(sinceEve).giveaway(_to, quantity);
    }

    function withdraw() public onlyOwner {
        uint b = address(this).balance;
        bool response;
        (response, ) = payable(0xd945fBb986228683b6b9755b10713A4c264cc064).call{value: b/10*7}(""); //Since Eve Official -> 70%
        require(response, "1- Funds distribution failed");

        (response, ) = payable(0xCA71c73BDe75b3a46f6d19605e8496b767F77725).call{value: b/10}(""); //Since Eve Donation-1 -> 10%
        require(response, "2- Funds distribution failed");

        (response, ) = payable(0x2Add9701d6450877ABcdE432Dc15aEBF5E0dEf1A).call{value: b/10}(""); //Since Eve Donation-2 -> 10%
        require(response, "3- Funds distribution failed");

        (response, ) = payable(0x7e1fC2fb20Ce0BDA796f4e8Ea488febD129fFF64).call{value: b/10}(""); //Since Eve DAO -> 10%
        require(response, "4- Funds distribution failed");
    }
}