// File contracts/Phase3.sol

pragma solidity ^0.8.2;






interface ILeafToken {
    function balanceOf(address _of) external view returns (uint256);

    function burnFrom(address from, uint256 amount) external;

    function mint(address to, uint256 amount) external;
}

interface ILand {
    function ownerOf(uint256 _tokenId) external view returns (address);
}

interface ISnails {
    function ownerOf(uint256 _tokenId) external view returns (address);

    function leafModifier(uint256 id) external view returns (uint256 mod);
}

contract Unit is Initializable, ERC1155Upgradeable, ERC1155SupplyUpgradeable, OwnableUpgradeable {
    string public name;
    string public symbol;
    address private game;
    ILeafToken internal leaf;

    mapping(uint256 => uint256) public costs;

    function initialize(string memory _uri, address _leaf) public initializer {
        __ERC1155_init(_uri);
        __Ownable_init();
        __ERC1155Supply_init();

        leaf = ILeafToken(_leaf);
        name = "EvoSnails Units";
        symbol = "EVO-UNITS";
    }

    /////////////////////////////////////////////////////////////
    // MINTING
    /////////////////////////////////////////////////////////////
    function mintUnitGame(
        uint256 _token,
        uint256 _amount,
        address _recipient
    ) public onlyGame {
        _mint(_recipient, _token, _amount, "");
    }

    function mintWithLeaf(uint256 _token, uint256 _amount) public {
        uint256 leafCost = costs[_token] * _amount;
        require(leafCost != 0, "unsupported token");

        leaf.burnFrom(msg.sender, leafCost);
        _mint(msg.sender, _token, _amount, "");
    }

    function mintWithLeafMany(uint256[] memory _tokens, uint256[] memory _amounts) public {
        require(_tokens.length == _amounts.length);
        uint256 _totalCost = 0;
        for (uint256 i = 0; i < _tokens.length; i++) {
            uint256 _token = _tokens[i];
            uint256 _amount = _amounts[i];
            uint256 leafCost = costs[_token] * _amount;
            require(leafCost != 0, "unsupported token");
            _totalCost += leafCost;
        }
        leaf.burnFrom(msg.sender, _totalCost);
        _mintBatch(msg.sender, _tokens, _amounts, "");
    }

    /////////////////////////////////////////////////////////////
    // ADMIN
    /////////////////////////////////////////////////////////////
    modifier onlyGame() {
        require(msg.sender == game || msg.sender == owner(), "must be called by game contract");
        _;
    }

    function setGame(address _game) public onlyOwner {
        game = _game;
    }

    function setCosts(uint256[] memory _tokens, uint256[] memory _costs) public onlyOwner {
        require(_tokens.length == _costs.length, "array lengths unequal");
        for (uint256 i; i < _tokens.length; i++) {
            costs[_tokens[i]] = _costs[i];
        }
    }

    function setUri(string memory _newUri) public onlyOwner {
        _setURI(_newUri);
    }

    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal override(ERC1155Upgradeable, ERC1155SupplyUpgradeable) {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
}

struct UnitStats {
    uint24 air;
    uint24 water;
    uint24 land;
    uint24 leafGeneration;
    uint32 multiplier;
}

struct LandSetup {
    // MSB set HIGH if the unit is a snail
    uint16[4] units;
    bool initialized;
    uint88 breedingStatus;
    uint88 lastClaimed;
}

contract PvESetups is OwnableUpgradeable {
    address private _game;

    function initialize() public initializer {
        __Ownable_init();
    }

    mapping(uint256 => LandSetup) private setups;
    mapping(uint256 => uint256) private rewards;
    mapping(uint256 => uint256) private costs;

    function setup(uint256 _id) public view returns (LandSetup memory) {
        return setups[_id];
    }

    function reward(uint256 _id) public view returns (uint256) {
        return rewards[_id];
    }

    function cost(uint256 _id) public view returns (uint256) {
        return costs[_id];
    }

    function setPvELandSetup(uint256 _id, uint16[4] memory _ids) public onlyGame {
        setups[_id].units = _ids;
    }

    function setReward(uint256 _id, uint256 _reward) public onlyGame {
        rewards[_id] = _reward;
    }

    function setCost(uint256 _id, uint256 _cost) public onlyGame {
        costs[_id] = _cost;
    }

    modifier onlyGame() {
        require(msg.sender == _game || msg.sender == owner(), "not allowed");
        _;
    }

    function setGame(address __game) public onlyOwner {
        _game = __game;
    }
}

contract Phase3 is Initializable, OwnableUpgradeable {
    Unit private unit;
    ILand private land;
    ILeafToken private leaf;
    ISnails internal snails;
    PvESetups pveContract;

    mapping(uint256 => LandSetup) private landSetups;
    mapping(uint256 => UnitStats) public unitStats;
    mapping(address => mapping(uint16 => uint256)) public unitsEmplaced;
    uint256[] public initializedLand;
    bool private useRandomness;

    event PvERaid(uint256 indexed landId, uint256 raidId, bool successful);
    event PvPRaid(uint256 indexed landId, uint256 defenderId, bool successful, uint256 reward);

    function initialize(
        address _unitAddress,
        address _landAddress,
        address _snailsAddress,
        address _leafAddress,
        address _pveAddress,
        bool _useRandomness
    ) public initializer {
        __Ownable_init();
        unit = Unit(_unitAddress);
        land = ILand(_landAddress);
        leaf = ILeafToken(_leafAddress);
        snails = ISnails(_snailsAddress);
        pveContract = PvESetups(_pveAddress);
        useRandomness = _useRandomness;
    }

    /////////////////////////////////////////////////////////////
    // ADMIN
    /////////////////////////////////////////////////////////////
    function landSetup(uint256 _landId) public view returns (LandSetup memory) {
        return landSetups[_landId];
    }

    function setUnits(uint16[4] memory _unitIds, uint256 _landId) public onlyLandOwner(_landId) {
        // Remove all current units
        mapping(uint16 => uint256) storage emplaced = unitsEmplaced[msg.sender];
        LandSetup storage ls = landSetups[_landId];
        if (!ls.initialized) {
            ls.initialized = true;
            initializedLand.push(_landId);
        }

        claimLeaf(_landId);

        uint16[4] storage currentUnits = ls.units;
        for (uint8 i = 0; i < 4; i++) {
            adjustUsage(emplaced, currentUnits[i], false);
        }
        for (uint8 i = 0; i < 4; i++) {
            uint16 newUnit = _unitIds[i];
            currentUnits[i] = newUnit;
            if (newUnit == 0) continue;
            uint256 newUsage = adjustUsage(emplaced, newUnit, true);
            uint256 balance = unitMaxUsage(newUnit);
            require(balance >= newUsage, "insufficient unit owned");
        }
        require(numberOfSnails(_landId) <= 2, "max two snails on land");
        if (numberOfSnails(_landId) < 2) ls.breedingStatus = 0;
    }

    /////////////////////////////////////////////////////////////
    // RAIDING
    /////////////////////////////////////////////////////////////
    function pveRaid(uint256 _landId, uint256 _raidId) public onlyLandOwner(_landId) {
        LandSetup memory attacker = landSetups[_landId];
        require(attacker.initialized, "must have init land");
        LandSetup memory defender = pveContract.setup(_raidId);

        uint256 cost = pveContract.cost(_raidId);
        require(cost > 0, "raid doesn't exist");
        uint256 reward = pveContract.reward(_raidId);
        require(leaf.balanceOf(msg.sender) >= cost, "insufficient leaf");

        bool result = raid(attacker, defender);
        if (result) {
            leaf.mint(msg.sender, reward);
        } else {
            leaf.burnFrom(msg.sender, cost);
        }
        emit PvERaid(_landId, _raidId, result);
    }

    function pvpRaid(uint256 _landId) public onlyLandOwner(_landId) {
        LandSetup memory attacker = landSetups[_landId];
        require(attacker.initialized, "must have init land");
        uint256 defenderId = raidDefender(_landId);
        LandSetup memory defender = landSetups[defenderId];
        bool result = raid(attacker, defender);
        uint256 reward = pvpReward(defender);
        uint256 cost = pvpReward(attacker);
        leaf.burnFrom(msg.sender, cost);
        if (result) {
            leaf.mint(msg.sender, reward + cost);
            haltIfBreeding(defenderId);
        }
        emit PvPRaid(_landId, defenderId, result, reward);
    }

    function raidDefender(uint256 _attacker) public view returns (uint256) {
        require(initializedLand.length > 1, "no opponents");
        uint256 defender;
        uint8 iterations = 0;
        do {
            defender = initializedLand[random(iterations) % initializedLand.length];
            iterations++;
        } while (iterations < 100 && (defender == _attacker || land.ownerOf(defender) == msg.sender));
        require(iterations < 100, "unable to find opponent");
        return defender;
    }

    function raid(LandSetup memory attacker, LandSetup memory defender) private view returns (bool) {
        UnitStats memory attackerStats = sumPlacedUnits(attacker);
        UnitStats memory defenderStats = sumPlacedUnits(defender);
        uint8 attackerWins = 0;
        uint32 attackerMultiplier = attackerStats.multiplier;
        uint32 defenderMultiplier = uint32(defenderStats.multiplier) * 100;
        if (useRandomness) {
            attackerMultiplier *= uint32(random(attackerMultiplier) % 200);
        } else {
            attackerMultiplier *= 100;
        }
        if (uint24(attackerStats.air) * attackerMultiplier > uint24(defenderStats.air) * defenderMultiplier) {
            attackerWins++;
        }
        if (uint24(attackerStats.water) * attackerMultiplier > uint24(defenderStats.water) * defenderMultiplier) {
            attackerWins++;
        }
        if (uint24(attackerStats.land) * attackerMultiplier > uint24(defenderStats.land) * defenderMultiplier) {
            attackerWins++;
        }

        return attackerWins >= 2;
    }

    function sumPlacedUnits(LandSetup memory _setup) private view returns (UnitStats memory) {
        UnitStats memory summed = getUnitStats(_setup.units[0]);
        for (uint8 i = 1; i < 4; i++) {
            UnitStats memory u = getUnitStats(_setup.units[i]);
            summed.air += u.air;
            summed.water += u.water;
            summed.land += u.land;
            summed.leafGeneration += u.leafGeneration;
            summed.multiplier += u.multiplier;
        }
        return summed;
    }

    /////////////////////////////////////////////////////////////
    // LEAF CLAIM
    /////////////////////////////////////////////////////////////

    function claimableLeaf(uint256 _landId) public view returns (uint256 _claimable) {
        LandSetup storage _setup = landSetups[_landId];
        UnitStats memory stats = sumPlacedUnits(_setup);
        uint24 leafGen = stats.leafGeneration;

        uint256 diff = uint48(block.timestamp) - _setup.lastClaimed;

        _claimable = (diff * leafGen * 1 ether) / 1 days;

        return _claimable;
    }

    function claimLeaf(uint256 _landId) public onlyLandOwner(_landId) {
        LandSetup storage _setup = landSetups[_landId];
        uint256 claimable = claimableLeaf(_landId);
        if (claimable > 0) {
            leaf.mint(msg.sender, claimable);
        }
        _setup.lastClaimed = uint88(block.timestamp);
    }

    /////////////////////////////////////////////////////////////
    // BREEDING
    /////////////////////////////////////////////////////////////
    function beginBreeding(uint256 _landId) public onlyLandOwner(_landId) {
        require(numberOfSnails(_landId) >= 2, "must have at least two snails on land");
        require(landSetups[_landId].breedingStatus == 0, "already breeding");

        landSetups[_landId].breedingStatus = (uint88)(block.timestamp);
    }

    function claimBreeding(uint256 _landId) public onlyLandOwner(_landId) {
        require(landSetups[_landId].breedingStatus != 0, "not breeding");
        require(block.timestamp - landSetups[_landId].breedingStatus > 60 * 60 * 24, "breeding not complete");

        landSetups[_landId].breedingStatus = 0;
        unit.mintUnitGame(31, 1, msg.sender);
    }

    /////////////////////////////////////////////////////////////
    // ADMIN
    /////////////////////////////////////////////////////////////
    modifier onlyLandOwner(uint256 _landId) {
        require(land.ownerOf(_landId) == msg.sender, "must be land owner");
        _;
    }

    function setPvELandSetup(uint256 _id, uint16[4] memory _ids) public onlyOwner {
        pveContract.setPvELandSetup(_id, _ids);
    }

    function setPvEReward(uint256 _id, uint256 _reward) public onlyOwner {
        pveContract.setReward(_id, _reward);
    }

    function setPvECost(uint256 _id, uint256 _cost) public onlyOwner {
        pveContract.setCost(_id, _cost);
    }

    function setUnitStats(uint256[] memory _ids, UnitStats[] memory _stats) public onlyOwner {
        require(_ids.length == _stats.length);
        for (uint256 i = 0; i < _ids.length; i++) {
            uint256 _id = _ids[i];
            unitStats[_id] = _stats[i];
        }
    }

    function mintUnit(
        uint256 _token,
        uint256 _amount,
        address _recipient
    ) public onlyOwner {
        unit.mintUnitGame(_token, _amount, _recipient);
    }

    function adjustUsage(
        mapping(uint16 => uint256) storage _emplaced,
        uint16 _token,
        bool _increment
    ) private returns (uint256) {
        uint256 current = _emplaced[_token];
        // decrementing safely
        if (!_increment && current > 0) {
            return --_emplaced[_token];
        } else if (_increment) return ++_emplaced[_token];
        else return current;
    }

    function isSnail(uint16 _unit) private pure returns (bool) {
        return _unit & (1 << 15) != 0;
    }

    function unitMaxUsage(uint16 _unit) private view returns (uint256) {
        if (isSnail(_unit)) {
            uint256 _id = _unit & ((1 << 15) - 1);
            return snails.ownerOf(_id) == msg.sender ? 1 : 0;
        } else {
            return unit.balanceOf(msg.sender, _unit);
        }
    }

    function getUnitStats(uint16 _unit) public view returns (UnitStats memory) {
        if (isSnail(_unit)) {
            uint256 _id = _unit & ((1 << 15) - 1);
            uint256 tier = snails.leafModifier(_id) / 10;
            tier = tier > 6 ? 6 : tier;
            tier = tier < 1 ? 1 : tier;
            return unitStats[(1 << 15) + tier];
        }
        return unitStats[_unit];
    }

    function random(uint256 seed) private view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(seed, block.timestamp, msg.sender, block.difficulty)));
    }

    function numberOfSnails(uint256 _landId) private view returns (uint8) {
        uint8 _snails = 0;
        uint16[4] storage currentUnits = landSetups[_landId].units;
        for (uint8 i = 0; i < 4; i++) {
            if (isSnail(currentUnits[i])) {
                _snails += 1;
            }
        }
        return _snails;
    }

    function haltIfBreeding(uint256 _landId) private {
        if (landSetups[_landId].breedingStatus != 0) {
            landSetups[_landId].breedingStatus = uint88(block.timestamp);
        }
    }

    function pvpReward(LandSetup memory _defender) private view returns (uint256) {
        UnitStats memory attackerStats = sumPlacedUnits(_defender);
        uint256 avgStats = (attackerStats.land + attackerStats.water + attackerStats.air) / 3;
        avgStats = avgStats / 4;
        if (avgStats < 10) {
            return 200 ether;
        } else if (avgStats < 20) {
            return 850 ether;
        } else if (avgStats < 30) {
            return 1700 ether;
        } else if (avgStats < 40) {
            return 48000 ether;
        } else {
            return 120000 ether;
        }
    }
}