pragma solidity ^0.8.17;

contract ReentrancyGuard {
  bool private rentrancy_lock = false;

  modifier nonReentrant() {
    require(!rentrancy_lock);
    rentrancy_lock = true;
    _;
    rentrancy_lock = false;
  }
}

