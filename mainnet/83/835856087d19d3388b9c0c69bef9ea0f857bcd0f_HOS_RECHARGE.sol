pragma solidity ^0.5.0;







contract HOS_RECHARGE is Ownable, Pausable {


    // @dev Total amount of Orders.
    uint256 private _totalOrders;

    // @dev Mapping from token index to ID.
    mapping (uint256 => uint256) private _overallTokenId;
    
    // @dev Mapping from token ID to index.
    mapping (uint256 => uint256) private _overallTokenIndex;
    
    using SafeMath for uint256;

    struct Order {
        address from;
        address to;
        uint256 amount;
        uint256 create_time;
    }

    Order[] orders;

    mapping(uint256 => address) private _orderOwner;

    mapping(address => uint256[]) private _ownedOrders;


    address public TOKEN_NFT_ADDRESS;

    constructor(address nft_address) public {
        TOKEN_NFT_ADDRESS = nft_address;
    }

    function SET_TOKEN_NFT_ADDRESS(address nft) public onlyOwner returns (address){
        TOKEN_NFT_ADDRESS = nft;
        return TOKEN_NFT_ADDRESS;
    }

    function withdraw_APENFT(uint256 amount, address to) public onlyOwner returns (bool){
        IERC20 token_nft = IERC20(TOKEN_NFT_ADDRESS);
        //return token_nft.transferFrom(address(this),to, amount);
        return token_nft.transfer(to, amount);
    }

    function approve_APENFT(uint256 amount, address to) public onlyOwner returns (bool){
        IERC20 token_nft = IERC20(TOKEN_NFT_ADDRESS);
        return token_nft.approve(to,amount);
    }

    function balanceOf_APENFT() public view returns (uint256) {
        IERC20 token_nft = IERC20(TOKEN_NFT_ADDRESS);
        return token_nft.balanceOf(address(this));
    }

    event Recharge(uint256 indexed order_id , address from ,address to ,uint256 amount);

    function Pay(address to, uint256 amount) public whenNotPaused returns (uint256 _orderId){
        require(amount >= 1e6);
        IERC20 token_nft = IERC20(TOKEN_NFT_ADDRESS);
        bool transfer_result = token_nft.transferFrom(msg.sender, address(this), amount);
        require(transfer_result == true);

        Order memory _order = Order(msg.sender, to, amount, now);
        _orderId = orders.push(_order) - 1;
        _orderOwner[_orderId] = msg.sender;
        _totalOrders = _totalOrders.add(1);

        emit Recharge(_orderId,msg.sender,to,amount);
    }

    modifier mustBeValidToken(uint256 _tokenId) {
        require(_orderOwner[_tokenId] != address(0));
        _;
    }

    function getOrder(
        uint256 _orderId
    )
    external
    view
    mustBeValidToken(_orderId)
    returns (address, address, uint256, uint256)
    {
        Order storage _order = orders[_orderId];
        return (_order.from, _order.to, _order.amount, _order.create_time);
    }

    function balanceOf(address _owner) external view returns (uint256) {
        require(_owner != address(0));
        return _ownedOrders[_owner].length;
    }

    function totalSupply() external view returns (uint256) {
        return _totalOrders;
    }

    function orderByIndex(uint256 _index) external view returns (uint256) {
        require(_index < _totalOrders);
        return _overallTokenId[_index];
    }

    function orderOfOwnerByIndex(address _owner, uint256 _index) external view returns (uint256 _orderId) {
        require(_owner != address(0));
        require(_index < _ownedOrders[_owner].length);
        return _ownedOrders[_owner][_index];
    }
}