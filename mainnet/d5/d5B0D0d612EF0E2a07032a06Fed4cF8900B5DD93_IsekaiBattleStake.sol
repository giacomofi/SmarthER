        Human,
        Elf,
        Dwarf,
        Demon,
        Merfolk,
        Therianthrope,
        Vampire,
        Angel,
        Unknown,
        Dragonewt,
        Monster
    }
    enum HeritageType {
        LowClass,
        MiddleClass,
        HighClass,
        Unknown
    }
    enum PersonalityType {
        Cool,
        Serious,
        Gentle,
        Optimistic,
        Rough,
        Diffident,
        Pessimistic,
        Passionate,
        Unknown,
        Frivolous,
        Confident
    }
    enum Phase {
        BeforeMint,
        WLMint,
        PublicMint,
        MintByTokens
    }

    struct Tokens {
        IERC20 SINN;
