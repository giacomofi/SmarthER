/**
 * @title A market for Arts on CHIZU
 */
contract CHIZUOrderMaker is Initializable, OrderFulfiller {
    /**
     * @dev Initialize function for upgradeable
     * @param _currencyManager The address of the used currency manager
     * @param _chizuCore The address of the contract defining roles for collections to use.
     */
