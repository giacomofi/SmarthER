pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

import "./MarketCore.sol";
import "../interfaces/ICurrencyManager.sol";
import "../interfaces/IRoles.sol";
import "../interfaces/ILazyMintable.sol";
import "hardhat/console.sol";

error OrderFulfiller_Is_Not_Node();
error OrderFulfiller_Signer_Address_Does_Not_Match();
error OrderFulfiller_Hash_Does_Not_Match();
error OrderFulfiller_Both_Are_Contracts();
error OrderFulfiller_Hash_Already_Exists();
error OrderFulfiller_Is_Deal_That_Can_Not();
error OrderFulfiller_Price_Is_Larger_Than_BalanceOf(uint256 balaceOf);
error OrderFulfiller_Time_Expired();

/**
 * @title Allows sellers to set a buy price of their NFTs that may be accepted and instantly transferred to the buyer.
 * @notice NFTs with a buy price set are escrowed in the market contract.
 */
abstract contract OrderFulfiller is MarketCore {
    using AddressUpgradeable for address payable;
    using ECDSA for bytes32;

    /// @dev Keep the order log to prevent duplicated order request
    /// There is almost no chance for different order to have same finalHash
    mapping(bytes32 => bool) public orderRecord;

    struct ValidateInfo {
        // hash to be used to validate
        bytes32 hashValue;
        // Signature created through hash
        bytes signature;
    }

    struct NFTInfo {
        // owner of NFT
        address account;
        // The address of colletion for NFT
        address collectionAddress;
        // The tokenId to exchanger
        uint256 tokenId;
        // The amount of token
        uint256 amount;
    }

    struct LazyNFTInfo {
        // The address of colletion for NFT
        address collectionAddress;
        // The creator of NFT
        address creatorAddress;
        // The receiver of NFT
        address receiverAddress;
        // The policy of token
        uint16 policy;
        // The uri of token
        string uri;
        // lazy mint hash
        bytes32 hashValue;
    }

    struct CurrencyInfo {
        // The account that owns the current
        address account;
        // The address of currency
        address currencyAddress;
        // The price to pay
        uint256 price;
    }

    struct RoyaltyInfo {
        // The fee that chizu receives
        uint256 protocolFee;
        // The fee that creator receives
        uint256 creatorFee;
        // The fee that referrer receives
        uint256 referrerFee;
        // The revenue that seller receives
        uint256 sellerRev;
    }

    event FulfilledBetweenLazy(
        bytes32 indexed recordedHash,
        NFTInfo orderInfo,
        NFTInfo considerInfo
    );

    event FulfilledLazyAndCurrency(
        bytes32 indexed recordedHash,
        NFTInfo nftInfo,
        CurrencyInfo currencyInfo,
        RoyaltyInfo royaltyInfo
    );

    event FulfilledBetweenMinted(
        bytes32 indexed recordedHash,
        NFTInfo orderInfo,
        NFTInfo considerInfo
    );

    event FulfilledMintedAndCurrency(
        bytes32 indexed recordedHash,
        NFTInfo nftInfo,
        CurrencyInfo currencyInfo,
        RoyaltyInfo royaltyInfo
    );

    event FulfilledLazyAndMinted(
        bytes32 indexed recordedHash,
        NFTInfo lazyMintedNFTInfo,
        NFTInfo nftInfo
    );

    /**
     * @notice Used to exchange Lazyminted tokens
     * @dev The role of node must sign
     * @param nodeAddress The address of the node that sign
     * @param orderInfo The information of order
     * @param considerInfo The information of consider
     * @param exchangeLazyValidate The information to validate for exchange lazy mint
     */
    function fulfillBetweenLazy(
        //Node Info
        address nodeAddress,
        uint256 salt,
        //order Info
        LazyNFTInfo memory orderInfo,
        //consider Info
        LazyNFTInfo memory considerInfo,
        //validate info
        ValidateInfo memory exchangeLazyValidate,
        //time
        uint256 expiredAt
    ) public payable {
        if (expiredAt < block.timestamp) {
            revert OrderFulfiller_Time_Expired();
        }

        if (orderRecord[exchangeLazyValidate.hashValue]) {
            revert OrderFulfiller_Hash_Already_Exists();
        }
        if (!IRoles(chizuCore).isNode(nodeAddress)) {
            revert OrderFulfiller_Is_Not_Node();
        }

        bool success = _validateExchangeLazySignature(
            nodeAddress,
            salt,
            orderInfo,
            considerInfo,
            exchangeLazyValidate,
            expiredAt
        );
        require(success, "OrderFulfiller : Signature is wrong");

        NFTInfo memory orderNFT;
        NFTInfo memory considerNFT;

        orderNFT.collectionAddress = orderInfo.collectionAddress;
        orderNFT.account = orderInfo.creatorAddress;
        orderNFT.amount = 0;

        considerNFT.collectionAddress = considerInfo.collectionAddress;
        considerNFT.account = considerInfo.creatorAddress;
        considerNFT.amount = 0;

        orderNFT.tokenId = ILazyMintable(orderInfo.collectionAddress)
            .chizuMintFor(
                orderInfo.creatorAddress,
                orderInfo.receiverAddress,
                orderInfo.policy,
                orderInfo.uri,
                orderInfo.hashValue,
                expiredAt
            );

        considerNFT.tokenId = ILazyMintable(considerInfo.collectionAddress)
            .chizuMintFor(
                considerInfo.creatorAddress,
                considerInfo.receiverAddress,
                considerInfo.policy,
                considerInfo.uri,
                considerInfo.hashValue,
                expiredAt
            );

        orderRecord[exchangeLazyValidate.hashValue] = true;

        emit FulfilledBetweenLazy(
            exchangeLazyValidate.hashValue,
            orderNFT,
            considerNFT
        );
    }

    /**
     * @notice Used to exchange betwwen lazyminted token and currency
     * @notice Insufficient eths are automatically charged
     * @dev The role of node must sign
     * @param nodeAddress The address of the node that sign
     * @param salt Random value used for transactions
     * @param lazyNFTInfo The information of nft
     * @param currencyInfo The information of currency
     * @param buyLazyValidate The information to validate for buy lazy mint
     */
    function fulfillLazyAndCurrency(
        //Node Info
        address nodeAddress,
        uint256 salt,
        //nft Info
        LazyNFTInfo memory lazyNFTInfo,
        //currency Info
        CurrencyInfo memory currencyInfo,
        // valdate info
        ValidateInfo memory buyLazyValidate,
        //time
        uint256 expiredAt
    ) public payable {
        if (expiredAt < block.timestamp) {
            revert OrderFulfiller_Time_Expired();
        }
        if (orderRecord[buyLazyValidate.hashValue]) {
            revert OrderFulfiller_Hash_Already_Exists();
        }
        if (!IRoles(chizuCore).isNode(nodeAddress)) {
            revert OrderFulfiller_Is_Not_Node();
        }

        bool success = _validateBuyLazySignature(
            nodeAddress,
            salt,
            lazyNFTInfo,
            currencyInfo,
            buyLazyValidate,
            expiredAt
        );
        require(success, "OrderFulfiller : Signature is wrong");

        NFTInfo memory nftInfo;

        nftInfo.collectionAddress = lazyNFTInfo.collectionAddress;
        nftInfo.account = lazyNFTInfo.creatorAddress;
        nftInfo.amount = 0;

        nftInfo.tokenId = ILazyMintable(lazyNFTInfo.collectionAddress)
            .chizuMintFor(
                lazyNFTInfo.creatorAddress,
                lazyNFTInfo.receiverAddress,
                lazyNFTInfo.policy,
                lazyNFTInfo.uri,
                lazyNFTInfo.hashValue,
                expiredAt
            );

        if (currencyInfo.currencyAddress == CURRENCY_ETH && msg.value > 0) {
            currencyManager.depositETHFor{value: msg.value}(msg.sender);
        }

        uint256 purchaserBalance = ICurrencyManager(currencyManager).balanceOf(
            currencyInfo.currencyAddress,
            currencyInfo.account
        );

        if (currencyInfo.price > purchaserBalance) {
            revert OrderFulfiller_Price_Is_Larger_Than_BalanceOf(
                ICurrencyManager(currencyManager).balanceOf(
                    currencyInfo.currencyAddress,
                    currencyInfo.account
                )
            );
        }

        RoyaltyInfo memory royaltyInfo;
        // Since a checks the interface in _distributeFunds, interface check is omitted here.
        (
            royaltyInfo.protocolFee,
            royaltyInfo.creatorFee,
            royaltyInfo.referrerFee,
            royaltyInfo.sellerRev
        ) = _distributeFunds(
            lazyNFTInfo.collectionAddress,
            nftInfo.tokenId,
            lazyNFTInfo.creatorAddress,
            currencyInfo.account,
            currencyInfo.currencyAddress,
            currencyInfo.price
        );

        orderRecord[buyLazyValidate.hashValue] = true;

        emit FulfilledLazyAndCurrency(
            buyLazyValidate.hashValue,
            nftInfo,
            currencyInfo,
            royaltyInfo
        );
    }

    /**
     * @notice Used to exchange minted tokens
     * @dev The role of node must sign
     * @param nodeAddress The address of the node that sign
     * @param salt Random value used for transactions
     * @param orderInfo The information of order
     * @param considerInfo The information of consider
     * @param exchangeNFTValidate The information to validate for exchange
     */
    function fulfillBetweenMinted(
        address nodeAddress,
        uint256 salt,
        NFTInfo memory orderInfo,
        NFTInfo memory considerInfo,
        ValidateInfo memory exchangeNFTValidate,
        //time
        uint256 expiredAt
    ) public payable {
        if (expiredAt < block.timestamp) {
            revert OrderFulfiller_Time_Expired();
        }
        if (orderRecord[exchangeNFTValidate.hashValue]) {
            revert OrderFulfiller_Hash_Already_Exists();
        }
        if (!IRoles(chizuCore).isNode(nodeAddress)) {
            revert OrderFulfiller_Is_Not_Node();
        }

        bool success = _validateExchangeNFTSignature(
            nodeAddress,
            salt,
            exchangeNFTValidate,
            orderInfo,
            considerInfo,
            expiredAt
        );
        require(success, "OrderFulfiller : Signature is wrong");

        _exchangeNfts(
            //Orderer
            orderInfo.account,
            orderInfo.collectionAddress,
            orderInfo.tokenId,
            orderInfo.amount,
            //Considerer
            considerInfo.account,
            considerInfo.collectionAddress,
            considerInfo.tokenId,
            considerInfo.amount
        );

        orderRecord[exchangeNFTValidate.hashValue] = true;

        emit FulfilledBetweenMinted(
            exchangeNFTValidate.hashValue,
            orderInfo,
            considerInfo
        );
    }

    /**
     * @notice Used to exchange between minted tokens and currency
     * @notice Insufficient eths are automatically charged
     * @dev The role of node must sign
     * @param nodeAddress The address of the node that sign
     * @param salt Random value used for transactions
     * @param nftInfo The information of nft
     * @param currencyInfo The information of currency
     * @param buyNFTValidate The information to validate for buy
     */
    function fulfillMintedAndCurrency(
        address nodeAddress,
        uint256 salt,
        NFTInfo memory nftInfo,
        CurrencyInfo memory currencyInfo,
        ValidateInfo memory buyNFTValidate,
        //time
        uint256 expiredAt
    ) public payable {
        if (expiredAt < block.timestamp) {
            revert OrderFulfiller_Time_Expired();
        }
        if (orderRecord[buyNFTValidate.hashValue]) {
            revert OrderFulfiller_Hash_Already_Exists();
        }
        if (!IRoles(chizuCore).isNode(nodeAddress)) {
            revert OrderFulfiller_Is_Not_Node();
        }
        bool success = _validateBuyNFTSignature(
            nodeAddress,
            salt,
            buyNFTValidate,
            nftInfo,
            currencyInfo,
            expiredAt
        );
        require(success, "OrderFulfiller : Signature is wrong");

        RoyaltyInfo memory royaltyInfo;

        (
            royaltyInfo.protocolFee,
            royaltyInfo.creatorFee,
            royaltyInfo.referrerFee,
            royaltyInfo.sellerRev
        ) = _exchangeNftAndCurrency(
            nftInfo.account,
            nftInfo.collectionAddress,
            nftInfo.tokenId,
            nftInfo.amount,
            currencyInfo.account,
            currencyInfo.currencyAddress,
            currencyInfo.price
        );

        orderRecord[buyNFTValidate.hashValue] = true;

        emit FulfilledMintedAndCurrency(
            buyNFTValidate.hashValue,
            nftInfo,
            currencyInfo,
            royaltyInfo
        );
    }

    /**
     * @notice Used to exchange minted tokens
     * @dev The role of node must sign
     * @param nodeAddress The address of the node that sign
     * @param salt Random value used for transactions
     * @param lazyNFTInfo The information of lazyNFT
     * @param nftInfo The information of NFT
     * @param exchangeValidate The information to validate for exchange
     */
    function fulfillLazyAndMinted(
        address nodeAddress,
        uint256 salt,
        LazyNFTInfo memory lazyNFTInfo,
        NFTInfo memory nftInfo,
        ValidateInfo memory exchangeValidate,
        //time
        uint256 expiredAt
    ) public payable {
        if (expiredAt < block.timestamp) {
            revert OrderFulfiller_Time_Expired();
        }
        if (orderRecord[exchangeValidate.hashValue]) {
            revert OrderFulfiller_Hash_Already_Exists();
        }
        if (!IRoles(chizuCore).isNode(nodeAddress)) {
            revert OrderFulfiller_Is_Not_Node();
        }

        bool success = _validateLazyAndNFTSignature(
            nodeAddress,
            salt,
            exchangeValidate,
            lazyNFTInfo,
            nftInfo,
            expiredAt
        );
        require(success, "OrderFulfiller : Signature is wrong");

        NFTInfo memory lazyMintedNFT;

        lazyMintedNFT.collectionAddress = lazyNFTInfo.collectionAddress;
        lazyMintedNFT.account = lazyNFTInfo.creatorAddress;
        lazyMintedNFT.amount = 0;

        lazyMintedNFT.tokenId = ILazyMintable(lazyNFTInfo.collectionAddress)
            .chizuMintFor(
                lazyNFTInfo.creatorAddress,
                lazyNFTInfo.receiverAddress,
                lazyNFTInfo.policy,
                lazyNFTInfo.uri,
                lazyNFTInfo.hashValue,
                expiredAt
            );

        _transferNftFrom(
            nftInfo.collectionAddress,
            nftInfo.tokenId,
            nftInfo.amount,
            nftInfo.account,
            lazyNFTInfo.creatorAddress
        );

        orderRecord[exchangeValidate.hashValue] = true;

        emit FulfilledBetweenMinted(
            exchangeValidate.hashValue,
            lazyMintedNFT,
            nftInfo
        );
    }

    /**
     * ==========================
     * Internal Function
     * ==========================
     */

