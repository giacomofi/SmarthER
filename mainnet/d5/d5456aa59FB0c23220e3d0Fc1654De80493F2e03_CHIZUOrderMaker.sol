pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "../Constants.sol";
import "../interfaces/ICurrencyManager.sol";
import "../interfaces/ICHIZUCore.sol";
import "../interfaces/IGetRoyalties.sol";
import "../interfaces/IRoyaltyInfo.sol";
import "../interfaces/ITokenCreator.sol";
import "../interfaces/IOwnable.sol";
import "../interfaces/IRoles.sol";
import "../interfaces/IDeriativeRoyaltyInfo.sol";
import "../OZ/OZERC165Checker.sol";
import "hardhat/console.sol";

error CHIZUMarketCore_Address_Is_Not_A_Contract();
error CHIZUMarketCore_Insufficient_Available_Funds(uint256 amount);
error CHIZUMarketCore_Asset_Does_Not_Owned_By(
    address collection,
    uint256 tokenId
);
error CHIZUMarketCore_Is_Not_Owner();
error CHIZUMarketCore_Is_Not_Approved();
error CHIZUMarketCore_Amount_Is_Larger_Than_BalanceOf(uint256 balaceOf);
error CHIZUMarketCore_Price_Is_Larger_Than_BalanceOf(uint256 balaceOf);

/**
 * @title A place for common modifiers and functions used by various CHIZUMarket.
 */
abstract contract MarketCore is Initializable {
    event Exchange_NFT_Complete();
    event Exchange_NFT_And_Currency_Complete();

    using AddressUpgradeable for address;
    using OZERC165Checker for address;

    ICurrencyManager internal currencyManager;
    address internal chizuCore;

    modifier onlyAdmin() {
        if (!(IRoles(chizuCore).isAdmin(msg.sender))) {
            revert CHIZUMarketCore_Address_Is_Not_A_Contract();
        }
        _;
    }

    function _initializeMarketCore(address _currencyManager, address _chizuCore)
        internal
        onlyInitializing
    {
        if (!_currencyManager.isContract()) {
            revert CHIZUMarketCore_Address_Is_Not_A_Contract();
        }
        currencyManager = ICurrencyManager(_currencyManager);
        chizuCore = _chizuCore;
    }

    function getCurrencyManagerAddress()
        external
        view
        returns (address currencyManagerAddress)
    {
        currencyManagerAddress = address(currencyManager);
    }

    function getChizuCoreAddress()
        external
        view
        returns (address chizuCoreAddress)
    {
        chizuCoreAddress = address(chizuCore);
    }

    /**
     * @notice Find the fee ditributed to each role
     * @param contractAddress The address of collection
     * @param tokenId The token Id
     * @param seller The seller of token
     * @param price The traded prcie
     * @return protocolFee Fees distributed to chizu
     * @return creator The creator of token
     * @return creatorFee Fees distributed to creator
     * @return referrer The referrer of token
     * @return referrerFee Fees distributed to referrer
     */
    function getFees(
        address contractAddress,
        uint256 tokenId,
        address seller,
        uint256 price
    )
        external
        view
        returns (
            uint256 protocolFee,
            address creator,
            uint256 creatorFee,
            address referrer,
            uint256 referrerFee
        )
    {
        (protocolFee, creator, creatorFee, referrer, referrerFee) = _getFees(
            contractAddress,
            tokenId,
            seller,
            price
        );
    }

    /**
     * @dev It's internal function to get the holder of the collection
     */
    function _getHolderForERC721(address collection, uint256 tokenId)
        internal
        view
        virtual
        returns (address holder)
    {
        holder = IERC721(collection).ownerOf(tokenId);
    }

    /**
     * @dev  It's internal function to get the balance of erc1155
     */
    function _getBalanceOfERC1155(
        address collection,
        address account,
        uint256 tokenId
    ) internal view virtual returns (uint256 balance) {
        balance = IERC1155(collection).balanceOf(account, tokenId);
    }

    /**
     * @dev  It's internal function to get the balance of account for specific currency
     */
    function _getPayerBalanceOf(address currency, address account)
        internal
        view
        virtual
        returns (uint256 balance)
    {
        balance = currencyManager.balanceOf(currency, account);
    }

    /**
     * @dev  It's internal function to exchange nfts
     */
    function _exchangeNfts(
        //Orderer
        address orderAddress,
        address orderContract,
        uint256 orderTokenId,
        uint256 orderAmount,
        //Considerer
        address considerAddress,
        address considerContract,
        uint256 considerTokenId,
        uint256 considerAmount
    ) internal {
        _transferNftFrom(
            orderContract,
            orderTokenId,
            orderAmount,
            orderAddress,
            considerAddress
        );

        _transferNftFrom(
            considerContract,
            considerTokenId,
            considerAmount,
            considerAddress,
            orderAddress
        );

        emit Exchange_NFT_Complete();
    }

    /**
     * @dev  It's internal function to transfer nft
     */
    function _transferNftFrom(
        address contractAddress,
        uint256 tokenId,
        uint256 amount,
        address fromAddress,
        address toAddress
    ) internal {
        if (amount == ERC721_RESERVED_AMOUNT) {
            /**
             * ======================================
             * Case 1 : ERC721
             * ======================================
             * @dev amount can not be zero
             * Therefore, if amount == 0, assume it as ERC721
             */
            if (fromAddress != IERC721(contractAddress).ownerOf(tokenId)) {
                revert CHIZUMarketCore_Is_Not_Owner();
            }
            IERC721(contractAddress).transferFrom(
                fromAddress,
                toAddress,
                tokenId
            );
        } else {
            /**
             * ======================================
             * Case 2 : ERC1155
             * ======================================
             */
            if (
                amount >
                IERC1155(contractAddress).balanceOf(fromAddress, tokenId)
            ) {
                revert CHIZUMarketCore_Amount_Is_Larger_Than_BalanceOf(
                    IERC1155(contractAddress).balanceOf(fromAddress, tokenId)
                );
            }
            if (
                !IERC1155(contractAddress).isApprovedForAll(
                    fromAddress,
                    address(this)
                )
            ) {
                revert CHIZUMarketCore_Is_Not_Approved();
            }

            IERC1155(contractAddress).safeTransferFrom(
                fromAddress,
                toAddress,
                tokenId,
                amount,
                new bytes(0)
            );
        }
    }

    /**
     * @dev  It's internal function to exchange nft and currency
     */
    function _exchangeNftAndCurrency(
        // NFT seller
        address sellerAddress,
        address contractAddress,
        uint256 tokenId,
        uint256 amount,
        // Purchaser
        address purchaserAddress,
        address currencyContract,
        uint256 price
    )
        internal
        returns (
            uint256 protocolFee,
            uint256 creatorFee,
            uint256 referrerFee,
            uint256 sellerRev
        )
    {
        /**
         * ================================================
         * Step 1 : Check if purchaser has enough balance
         * ================================================
         */
        if (currencyContract == CURRENCY_ETH && msg.value > 0) {
            currencyManager.depositETHFor{value: msg.value}(msg.sender);
        }

        uint256 purchaserBalance = ICurrencyManager(currencyManager).balanceOf(
            currencyContract,
            purchaserAddress
        );

        if (price > purchaserBalance) {
            revert CHIZUMarketCore_Price_Is_Larger_Than_BalanceOf(
                ICurrencyManager(currencyManager).balanceOf(
                    currencyContract,
                    purchaserAddress
                )
            );
        }
        /**
         * ================================================
         * Step 2 : Transfer the NFT to the purchaser
         * ================================================
         * If it's not approved, it will throw.
         */
        _transferNftFrom(
            contractAddress,
            tokenId,
            amount,
            sellerAddress,
            purchaserAddress
        );

        /**
         * ================================================
         * Step 3 : Purchaser pay currency
         * ================================================
         */

        (protocolFee, creatorFee, referrerFee, sellerRev) = _distributeFunds(
            contractAddress,
            tokenId,
            payable(sellerAddress),
            purchaserAddress,
            currencyContract,
            price
        );
        emit Exchange_NFT_And_Currency_Complete();
    }

    /**
     * @dev  It's internal function to distribute funds
     */
    function _distributeFunds(
        address contractAddress,
        uint256 tokenId,
        address seller,
        address purchaserAddress,
        address currencyContract,
        uint256 price
    )
        internal
        returns (
            uint256 protocolFee,
            uint256 creatorFee,
            uint256 referrerFee,
            uint256 sellerRev
        )
    {
        // 1) Variable declarations
        address creator;
        address referrer;

        // 2) Get fee info
        (protocolFee, creator, creatorFee, referrer, referrerFee) = _getFees(
            contractAddress,
            tokenId,
            seller,
            price
        );

        // 3) Distribute funds
        // Protocol Fee
        currencyManager.chizuReduceCurrencyFrom(
            currencyContract,
            purchaserAddress,
            protocolFee
        );
        // Creator Fee
        if (creator != address(0) && creatorFee != 0) {
            currencyManager.chizuTransferCurrencyFrom(
                currencyContract,
                purchaserAddress,
                creator,
                creatorFee
            );
        }

        // Referrer Fee
        if (referrer != address(0) && referrerFee != 0) {
            currencyManager.chizuTransferCurrencyFrom(
                currencyContract,
                purchaserAddress,
                referrer,
                referrerFee
            );
        }

        // Seller Rev
        if (seller != address(0)) {
            sellerRev = price - protocolFee - creatorFee - referrerFee;
            currencyManager.chizuTransferCurrencyFrom(
                currencyContract,
                purchaserAddress,
                seller,
                sellerRev
            );
        }
    }

    /**
     * @dev  It's internal function to get fees
     */
    function _getFees(
        address contractAddress,
        uint256 tokenId,
        address seller,
        uint256 price
    )
        internal
        view
        returns (
            uint256 protocolFee,
            address creator,
            uint256 creatorFee,
            address referrer,
            uint256 referrerFee
        )
    {
        protocolFee = (price * 2) / 100;
        if (
            contractAddress.supportsERC165Interface(
                type(IRoyaltyInfo).interfaceId
            )
        ) {
            (creator, creatorFee) = IRoyaltyInfo(contractAddress).royaltyInfo(
                tokenId,
                price
            );
        }

        if (creator == address(0)) {
            if (
                contractAddress.supportsERC165Interface(
                    type(IGetRoyalties).interfaceId
                )
            ) {
                address payable[] memory recipients;
                uint256[] memory fees;

                (recipients, fees) = IGetRoyalties(contractAddress)
                    .getRoyalties(tokenId, price);

                if (recipients.length >= 1) {
                    creator = recipients[0];
                    creatorFee = fees[0];
                }

                if (recipients.length >= 2) {
                    referrer = recipients[1];
                    referrerFee = fees[1];
                }
            }
        }

        if (referrer == address(0)) {
            if (
                contractAddress.supportsERC165Interface(
                    type(IDeriativeRoyaltyInfo).interfaceId
                )
            ) {
                (referrer, referrerFee) = IDeriativeRoyaltyInfo(contractAddress)
                    .deriativeRoyaltyInfo(tokenId, price);
            }
        }

        if (creator == address(0)) {
            if (
                contractAddress.supportsInterface(
                    type(ITokenCreator).interfaceId
                )
            ) {
                creator = ITokenCreator(contractAddress).tokenCreator(tokenId);
                if (referrerFee != 0) {
                    creatorFee = (price * 2) / 100;
                } else {
                    creatorFee = (price * 5) / 100;
                }
            }
        }

        if (creator == address(0)) {
            if (contractAddress.supportsInterface(type(IOwnable).interfaceId)) {
                creator = IOwnable(contractAddress).owner();
                if (referrerFee != 0) {
                    creatorFee = (price * 2) / 100;
                } else {
                    creatorFee = (price * 5) / 100;
                }
            }
        }
        // To prevent transfer to duplicated
        if (seller == creator) {
            creator = address(0);
            creatorFee = 0;
        }
    }

    uint256[999] private __gap;
}

