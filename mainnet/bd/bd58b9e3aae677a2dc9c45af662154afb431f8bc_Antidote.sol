    mapping(uint256 => bool) isUsedCagc;
    mapping(uint256 => bool) isUsedMagc;

    function name() external pure returns(string memory) {
        return "MAGC Antidote";
    }

    function symbol() external pure returns(string memory) {
        return "CURE";
    }
    
    constructor() ERC1155("https://metadata.magcantidote.art/{id}.json") {
        cagc = IERC721(0xab28f24812dC22FfCD6FF280f94a3EF5CE1AbAf2);
        magc = IERC721(0xE98672402b6297B675Bc14D4A4363AD7c84eDf02);
        chad = IERC20(0x8408abB1BF0812603A8A57678048187166a25E92);
        upgradeCost = 300000000000000000000;
        maxSupply = 2924;
        stakingContract = ISTAKE(0xC8a3A8db5295449e7B751aE91549D78D0BB83755);
    }

    function upgrade(uint256 _magcId) external whenNotPaused nonReentrant {
        require(isUpgraded[_magcId] == false , "MAGC already upgraded");
        require(magc.ownerOf(_magcId) == msg.sender, "Invalid MAGC Id");
        _burn(msg.sender, 0, 1);
        isUpgraded[_magcId] = true;
        emit upgraded(_magcId);
    }

    function claim(uint256 _cagcId, uint256 _magcId) external whenNotPaused nonReentrant {
        require(cagc.ownerOf(_cagcId) == msg.sender || stakingContract.stakings(_cagcId).owner == msg.sender, "Invalid CAGC Id");
        require(magc.ownerOf(_magcId) == msg.sender, "Invalid MAGC Id");
        require(isUsedCagc[_cagcId] == false, "Already claimed");
        require(isUsedMagc[_magcId] == false, "Already claimed");
        require(totalSupply(0) + 1 <= maxSupply, "Max supply reached");
        isUsedCagc[_cagcId] = true;
        isUsedMagc[_magcId] = true;
        _mint(msg.sender, 0, 1, "");
        chad.safeTransferFrom(msg.sender, owner(), upgradeCost);
    }

    function setURI(string memory newuri) public onlyOwner {
        _setURI(newuri);
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
        internal
        override(ERC1155, ERC1155Supply)
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }

    event upgraded(uint256 _magcId);
}