     * `amount`.
     */
    function burnFrom(address account, uint256 amount) public virtual {
        _approve(
            account,
            _msgSender(),
            _allowances[account][_msgSender()]-amount
        );
        _burn(account, amount);
    }
    receive() external payable {}
}

