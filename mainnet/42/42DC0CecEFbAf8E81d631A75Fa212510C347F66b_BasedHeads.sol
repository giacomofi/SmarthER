pragma solidity ^0.8.17;

error MaxSupplyExceeded(uint256 maxSupply);
error SaleStateLimitExceeded(uint256 limit);
error IncorrectFunds(uint256 fundsRequired);
error InvalidSignature();
error SaleStateClosed();
