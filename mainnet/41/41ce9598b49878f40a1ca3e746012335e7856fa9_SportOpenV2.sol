   * @notice Cancel a subscription
   * @param subId - ID of the subscription
   * @param to - Where to send the remaining LINK to
   */
  function cancelSubscription(uint64 subId, address to) external;
}

// File: contracts/mysterybox/SportOpenV2.sol

pragma solidity ^0.8.0;













contract SportOpenV2 is Ownable, ReentrancyGuard, VRFConsumerBaseUpgradableV2 {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.UintSet;

    VRFCoordinatorV2Interface public COORDINATOR;
    uint64 subscriptionId;
    uint32 public callbackGasLimit;
    bytes32 internal keyHash;

    mapping(uint256 => address) public callerByReqId;
    mapping(uint256 => uint256) public boxByReqId;
    mapping(uint256 => bool) public openByReqId;

    bool private initialized;
    uint256 public openStartTime;

    address public mysteryBox;
    address public sportswar;
    address public randomSeed;

    mapping(uint256 => uint256) public claimedForTeam;
    mapping(uint256 => mapping(uint256 => uint256)) public claimedLevelForTeam;
    mapping(uint256 => uint256) public levelAmounts;
    mapping(uint256 => uint256) public levelRate;
    uint256 public amountForTeam;
    // set 1-32
    EnumerableSet.UintSet private spareTeam;

    event OPEN(
        address indexed sender,
        uint256 boxId,
        uint256 requestId
    );

    event OpenResult(
        uint256 requestId,
        uint256 tokenId,
        uint256 team,
        uint256 level);

    function initialize(
        address _owner,
        uint256 _openStartTime,
        address _mysteryBox,
        address _sportswar,
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint32 _callbackGasLimit,
        uint32 _subscriptionId
    ) external {
        require(!initialized, "initialize: Already initialized!");
        _transferOwnership(_owner);
        openStartTime = _openStartTime;
        mysteryBox = _mysteryBox;
        sportswar = _sportswar;
        
        COORDINATOR = VRFCoordinatorV2Interface(_vrfCoordinator);
        VRFConsumerBaseUpgradableV2.vrfInitialize(
            _vrfCoordinator
        );
        keyHash = _keyHash;
        callbackGasLimit = _callbackGasLimit;
        subscriptionId = _subscriptionId;
        initialized = true;
    }

    function open(uint256 boxId) external nonReentrant returns(uint256){
        require(!Address.isContract(msg.sender), "can't call");
        require(msg.sender == tx.origin, "can't call");
        
        require(block.timestamp >= openStartTime, "not start to open box");
        IERC721(mysteryBox).safeTransferFrom(msg.sender, address(this), boxId);
        ISportswarMysteryBox(mysteryBox).burn(boxId);
        
        uint256 requestId = COORDINATOR.requestRandomWords(
            keyHash,
            subscriptionId,
            3,
            callbackGasLimit,
            1
        );

        callerByReqId[requestId] = msg.sender;
        boxByReqId[requestId] = boxId;
        openByReqId[requestId] = false;

        emit OPEN(msg.sender, boxId, requestId);
        return requestId;
    }
    
     /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomWords(
        uint256 requestId,
        uint256[] memory _randomWords) internal override {
        address addr = callerByReqId[requestId];
        openByReqId[requestId] = true;

        (uint256 team, uint256 level) = infoByRandom(_randomWords[0]);
        ISportswar.SportswarInfo memory info;
        info.team = team;
        info.level = level;
        uint256 tokenId = ISportswar(sportswar).mint(addr, info);

        emit OpenResult(requestId, tokenId, team, level);
    }

    function infoByRandom(uint256 seed) internal returns (uint256 team, uint256 level) {
        uint teamRandom = seed.mod(spareTeam.length());
        team = spareTeam.at(teamRandom);
        claimedForTeam[team]++;
        if (claimedForTeam[team] == amountForTeam) {
            spareTeam.remove(team);
        }

        uint256 levelRandom = seed.mod(amountForTeam);
        for (uint256 levle_ = 1; levle_ <= 5; levle_++) {
            if (levelRandom < levelRate[levle_]) {
                level = claimLevel(team, levle_);
                break;
            }
        }
        claimedLevelForTeam[team][level]++;
    }

    function claimLevel(uint256 team_,uint256 levle_) internal returns (uint256 level) {
        if (claimedLevelForTeam[team_][levle_] < levelAmounts[levle_]) { 
            level = levle_;
        }else{
            if(levle_ < 5) {
                level = claimLevel(team_, (levle_.add(1)));
            }else if(levle_ == 5) {
                level = claimLevel(team_, 1);
            }
        }
    }

    function setLevelAmount(uint256[] calldata _level) external onlyOwner{
        for (uint256 i = 0; i < _level.length; i++) {
            levelRate[i+1] = levelRate[i].add(_level[i]);
            levelAmounts[i+1] = _level[i];
            amountForTeam += _level[i];
        }
        for (uint256 j = 1; j < 33; j++) {
            spareTeam.add(j);
        }
    }

    function updateInfo(
        uint256 _openStartTime,
        address _mysteryBox,
        address _sportswar) external onlyOwner{
            openStartTime = _openStartTime;
            mysteryBox = _mysteryBox;
            sportswar = _sportswar;
    }

    function onERC721Received(
        address /*operator*/ ,
        address /*from*/ ,
        uint256 /*tokenId*/ ,
        bytes calldata  /*data*/)
        external pure returns (bytes4) {
        return this.onERC721Received.selector;
    }
}