interface AggregatorV3Interface {
  function decimals() external view returns (uint8);

  function description() external view returns (string memory);

  function version() external view returns (uint256);

  // getRoundData and latestRoundData should both raise "No data present"
  // if they do not have data to report, instead of returning unset values
  // which could be misinterpreted as actual reported values.
  function getRoundData(uint80 _roundId)
    external
    view
    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );

  function latestRoundData()
    external
    view
    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );
}
contract AsyncSale is ReentrancyGuard, Ownable {
    using SafeCast for int256;
    using SafeMath for uint256;
    uint256 public roundId;
    address public immutable safe = 0x827c80a62BCD47ce38BdE5BA65D3b93a1d58bb90;
    RoundType public currentRoundType;
    AggregatorV3Interface internal bnb_priceFeed;

    enum RoundType {
        SEED,
        PRIVATE,
        PUBLIC
    }
    event SaleRoundEnded(
        uint256 indexed roundId,
        uint256 tokenPrice,
        uint256 tokenSupply,
        uint256 tokensBuyed
    );

    event SaleRoundCreated(
        uint256 indexed roundId,
        uint256 tokenPrice,
        uint256 tokenSupply,
        uint256 startTime,
        uint256 endTime
    );
    
    event TokensPurchased(
        address indexed buyer,
        uint256 indexed roundId,
        uint256 tokensAmount
    );

    struct Purchase{
        address buyer;
        uint256 tokensAmount; // in usd
    }

    struct SaleRound {
        // There is already a global variable of the current price,
        // but here it is additionally so that it can be tracked in history.
        uint256 tokenPrice;
        uint256 tokenSupply; // in usd
        uint256 startTime;
        uint256 endTime;
        uint256 minPurchase; //in usd
        uint256 tokensSold;  //in usd
        RoundType roundType;
    }
    mapping(uint256 => SaleRound) public saleRounds;
    mapping(address=>bool) public isTokenAllowed;
    /* Mapping of Purchase of a wallet in specific round */
    mapping(address=>mapping(uint256=>Purchase)) public purchase; 

    constructor(){  
        bnb_priceFeed = AggregatorV3Interface(0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE);
        isTokenAllowed[0x0000000000000000000000000000000000000000] = true;
        isTokenAllowed[0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48] = true;
        isTokenAllowed[0xdAC17F958D2ee523a2206206994597C13D831ec7] = true;

    }

    modifier whenSaleOn {
        require(saleRounds[roundId].startTime<block.timestamp && saleRounds[roundId].endTime > block.timestamp, "Sale is not Live");
        _;
    }

    function getBNBPrice() public view returns (uint) {
        (
            , int price, , , 
        ) = bnb_priceFeed.latestRoundData();

        return price.toUint256();
    }

    function pfDecimals() public view returns(uint){
        return bnb_priceFeed.decimals();
    }

    function getBNBAmount(uint256 amountInUSD) public view returns(uint256){
        uint256 amt = (amountInUSD.mul(10**pfDecimals())).div(getBNBPrice());
        return amt;
    }

    function currentTokenPrice() public view returns(uint256){
        return saleRounds[roundId].tokenPrice;
    }
    function roundDurationLeft() public view whenSaleOn returns(uint256)  {
        return saleRounds[roundId].endTime.sub(block.timestamp);
    }
    function tokenSupplyforCurrentRound() public view returns(uint256){
        uint256 price = saleRounds[roundId].tokenPrice;
        uint256 tokenSupply_ = saleRounds[roundId].tokenSupply;
        return tokenSupply_.div(price);
    }
    function tokenLeftforCurrentRound() public view returns(uint256){
        uint256 price = saleRounds[roundId].tokenPrice;
        uint256 tokenSupply_ = saleRounds[roundId].tokenSupply;
        uint256 tokenSold_ = saleRounds[roundId].tokensSold;
        return (tokenSupply_.sub(tokenSold_)).div(price);
    }

    /* convinient for UI use */
    function getBuyerData(address wallet, uint256 roundId_) public view returns(address buyer, uint256 amount){
        return (purchase[wallet][roundId_].buyer, purchase[wallet][roundId_].tokensAmount);
    }

    function whitelistToken(address token_, bool status_) public onlyOwner {
        isTokenAllowed[token_] = status_;
    }

    function startSaleRound(uint256 tokenPrice_, uint256 totalSupply_, uint256 startTime_, uint256 endTime_, uint256 minPurchase_, RoundType roundType_) public onlyOwner {
        roundId++;
        saleRounds[roundId] = SaleRound(tokenPrice_, totalSupply_, startTime_, endTime_, minPurchase_, 0, roundType_);
        currentRoundType = roundType_;
        emit SaleRoundCreated(roundId, tokenPrice_, totalSupply_, startTime_, endTime_);
        SaleRound memory oldRound = saleRounds[roundId.sub(1)];
        emit SaleRoundEnded(roundId.sub(1), oldRound.tokenPrice, oldRound.tokenSupply, oldRound.tokensSold);
    }

    function buyTokens(uint256 amountInBUSD, address token_) public payable nonReentrant whenSaleOn {
        require(isTokenAllowed[token_]== true, "Token not allowed");
        SaleRound storage sale = saleRounds[roundId];
        require(amountInBUSD>=sale.minPurchase, "Buy more amount");
        require(sale.tokensSold.add(amountInBUSD)<=sale.tokenSupply, "Purchase would exceed totalSupply for the round");
        if(token_==address(0)){
            uint256 amt = (amountInBUSD.mul(10**pfDecimals())).div(getBNBPrice());
            require(msg.value == amt, "Send proper msg value");
            payable(safe).transfer(amt);
        } else {
            IERC20(token_).transferFrom(msg.sender, safe, amountInBUSD);
        }
        sale.tokensSold+=amountInBUSD;
        _buy(msg.sender, amountInBUSD);
    }

    function _buy(address buyer_, uint256 amount) private {
        purchase[buyer_][roundId].buyer = buyer_;
        purchase[buyer_][roundId].tokensAmount += amount;
        emit TokensPurchased(buyer_, roundId, amount);
    }

}
contract AsyncClaim is ReentrancyGuard, Ownable {
    using SafeMath for uint256;

    IERC20 public ASYNC;
    AsyncSale public tokenSale;
    mapping(address=>bool) private claimable;

    mapping(uint256=>uint256) public tokenPrice;

    event Claimed(address indexed wallet, uint256 amount);

    struct ClaimData {
        address wallet;
        uint256 amount;
    }

    constructor(IERC20 async, AsyncSale sale) {
        ASYNC = async;
        tokenSale = sale;
    }

    function setTokenPrice(uint256 roundId, uint256 price) public onlyOwner {
        tokenPrice[roundId] = price;
    }

    function checkClaimed(address wallet) public view returns(bool) {
        return claimable[wallet];
    }

    function checkClaimable(address wallet, uint256 roundId) public view returns(uint256) {
            (, uint256 amount ) = tokenSale.getBuyerData(wallet, roundId);
       
        uint256 asyncAmount = (amount.mul(10**18)).div(tokenPrice[roundId]);
        return asyncAmount;  
    }
    function checkUSD(address wallet, uint256 roundId) public view returns(uint256) {
            (, uint256 amount ) = tokenSale.getBuyerData(wallet, roundId);
       
        return amount;  
    }

    function checkPrice(uint256 roundId) public view returns(uint256) {
        return tokenPrice[roundId];  
    }

    function claim(uint256 roundId) public nonReentrant {
       (, uint256 amount ) = tokenSale.getBuyerData(msg.sender, roundId);
        uint256 asyncAmount = (amount.mul(10**18)).div(tokenPrice[roundId]);
        require(asyncAmount > 0 && !claimable[msg.sender], "No tokens available");
        claimable[msg.sender] = true;
        ASYNC.transfer(msg.sender,asyncAmount);
        emit Claimed(msg.sender, asyncAmount);
    }
}